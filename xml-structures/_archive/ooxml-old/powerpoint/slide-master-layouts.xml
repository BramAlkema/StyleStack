          <a:bodyPr/>
          <a:lstStyle/>
          <a:p>
            <a:pPr lvl="0"/>
            <a:r>
              <a:rPr lang="en-US" smtClean="0"/>
              <a:t>{{layout.twoContent.leftContent.placeholder}}</a:t>
            </a:r>
          </a:p>
        </p:txBody>
      </p:sp>
      
      <!-- Right content placeholder -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="4" name="Content Placeholder 3"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr>
            <p:ph sz="half" idx="2"/>
          </p:nvPr>
        </p:nvSpPr>
        
        <p:spPr>
          <a:xfrm>
            <a:off x="{{layout.twoContent.rightContent.position.x}}" y="{{layout.twoContent.rightContent.position.y}}"/>
            <a:ext cx="{{layout.twoContent.rightContent.size.width}}" cy="{{layout.twoContent.rightContent.size.height}}"/>
          </a:xfrm>
        </p:spPr>
        
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p>
            <a:pPr lvl="0"/>
            <a:r>
              <a:rPr lang="en-US" smtClean="0"/>
              <a:t>{{layout.twoContent.rightContent.placeholder}}</a:t>
            </a:r>
          </a:p>
        </p:txBody>
      </p:sp>
    </p:spTree>
  </p:cSld>
  
</p:sldLayout>