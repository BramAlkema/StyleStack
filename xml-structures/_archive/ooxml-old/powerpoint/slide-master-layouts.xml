          <a:xfrm>
            <a:off x="{{layout.twoContent.leftContent.position.x}}" y="{{layout.twoContent.leftContent.position.y}}"/>
            <a:ext cx="{{layout.twoContent.leftContent.size.width}}" cy="{{layout.twoContent.leftContent.size.height}}"/>
