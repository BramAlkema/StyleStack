<?xml version="1.0" encoding="UTF-8"?>
<!-- PowerPoint Slide Master and Layout Definitions with Complete OOXML Primitives -->
<!-- These provide the master layout structures that design tokens can patch against -->

<!-- SLIDE MASTER DEFINITION -->
<p:sldMaster xmlns:p="http://schemas.openxmlformats.org/presentationml/2006/main"
             xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main"
             xmlns:r="http://schemas.openxmlformats.org/officeDocument/2006/relationships">
  
  <!-- Common slide properties -->
  <p:cSld name="{{master.name}}">
    <!-- Background -->
    <p:bg>
      <p:bgPr>
        <a:solidFill>
          <a:srgbClr val="{{master.background.color}}"/>
        </a:solidFill>
        <a:effectLst/>
      </p:bgPr>
    </p:bg>
    
    <!-- Slide content area -->
    <p:spTree>
      <!-- Non-visual group shape properties -->
      <p:nvGrpSpPr>
        <p:cNvPr id="1" name=""/>
        <p:cNvGrpSpPr/>
        <p:nvPr/>
      </p:nvGrpSpPr>
      
      <!-- Group shape properties -->
      <p:grpSpPr>
        <a:xfrm>
          <a:off x="0" y="0"/>
          <a:ext cx="0" cy="0"/>
          <a:chOff x="0" y="0"/>
          <a:chExt cx="0" cy="0"/>
        </a:xfrm>
      </p:grpSpPr>
      
      <!-- Title Placeholder -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="{{master.title.id}}" name="Title Placeholder 1"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr>
            <p:ph type="title"/>
          </p:nvPr>
        </p:nvSpPr>
        
        <!-- Title placeholder positioning -->
        <p:spPr>
          <a:xfrm>
            <a:off x="{{master.title.position.x}}" y="{{master.title.position.y}}"/>
            <a:ext cx="{{master.title.size.width}}" cy="{{master.title.size.height}}"/>
          </a:xfrm>
          <a:prstGeom prst="rect">
            <a:avLst/>
          </a:prstGeom>
          <a:noFill/>
        </p:spPr>
        
        <!-- Title text properties -->
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle>
            <!-- Title text styling controlled by design tokens -->
            <a:lvl1pPr algn="{{master.title.alignment}}" 
                       defTabSz="{{master.title.tabSize}}" 
                       rtl="0" 
                       eaLnBrk="1" 
                       fontAlgn="auto" 
                       latinLnBrk="0" 
                       hangingPunct="1">
              <a:spcBef>
                <a:spcPct val="{{master.title.spacing.before}}"/>
              </a:spcBef>
              <a:buNone/>
              <a:defRPr sz="{{master.title.font.size}}" 
                        kern="1200" 
                        b="{{master.title.font.bold}}">
                <a:solidFill>
                  <a:schemeClr val="{{master.title.color.scheme}}"/>
                </a:solidFill>
                <a:latin typeface="{{master.title.font.family}}"/>
                <a:ea typeface="{{master.title.font.ea}}"/>
                <a:cs typeface="{{master.title.font.cs}}"/>
              </a:defRPr>
            </a:lvl1pPr>
          </a:lstStyle>
          
          <!-- Placeholder text -->
          <a:p>
            <a:r>
              <a:rPr lang="en-US" smtClean="0"/>
              <a:t>{{master.title.placeholder}}</a:t>
            </a:r>
            <a:endParaRPr lang="en-US"/>
          </a:p>
        </p:txBody>
      </p:sp>
      
      <!-- Content Placeholder -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="{{master.content.id}}" name="Content Placeholder 2"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr>
            <p:ph type="body"/>
          </p:nvPr>
        </p:nvSpPr>
        
        <!-- Content placeholder positioning -->
        <p:spPr>
          <a:xfrm>
            <a:off x="{{master.content.position.x}}" y="{{master.content.position.y}}"/>
            <a:ext cx="{{master.content.size.width}}" cy="{{master.content.size.height}}"/>
          </a:xfrm>
          <a:prstGeom prst="rect">
            <a:avLst/>
          </a:prstGeom>
          <a:noFill/>
        </p:spPr>
        
        <!-- Content text properties with multiple levels -->
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle>
            <!-- Level 1 bullet -->
            <a:lvl1pPr marL="{{master.content.level1.marginLeft}}" 
                       algn="{{master.content.level1.alignment}}" 
                       defTabSz="{{master.content.level1.tabSize}}" 
                       rtl="0" 
                       eaLnBrk="1" 
                       fontAlgn="auto" 
                       latinLnBrk="0" 
                       hangingPunct="1">
              <a:spcBef>
                <a:spcPct val="{{master.content.level1.spacing.before}}"/>
              </a:spcBef>
              <a:buFont typeface="{{master.content.level1.bullet.font}}"/>
              <a:buChar char="{{master.content.level1.bullet.char}}"/>
              <a:defRPr sz="{{master.content.level1.font.size}}" kern="1200">
                <a:solidFill>
                  <a:schemeClr val="{{master.content.level1.color.scheme}}"/>
                </a:solidFill>
                <a:latin typeface="{{master.content.level1.font.family}}"/>
                <a:ea typeface="{{master.content.level1.font.ea}}"/>
                <a:cs typeface="{{master.content.level1.font.cs}}"/>
              </a:defRPr>
            </a:lvl1pPr>
            
            <!-- Level 2 bullet -->
            <a:lvl2pPr marL="{{master.content.level2.marginLeft}}" 
                       algn="{{master.content.level2.alignment}}" 
                       defTabSz="{{master.content.level2.tabSize}}" 
                       rtl="0" 
                       eaLnBrk="1" 
                       fontAlgn="auto" 
                       latinLnBrk="0" 
                       hangingPunct="1">
              <a:spcBef>
                <a:spcPct val="{{master.content.level2.spacing.before}}"/>
              </a:spcBef>
              <a:buFont typeface="{{master.content.level2.bullet.font}}"/>
              <a:buChar char="{{master.content.level2.bullet.char}}"/>
              <a:defRPr sz="{{master.content.level2.font.size}}" kern="1200">
                <a:solidFill>
                  <a:schemeClr val="{{master.content.level2.color.scheme}}"/>
                </a:solidFill>
                <a:latin typeface="{{master.content.level2.font.family}}"/>
                <a:ea typeface="{{master.content.level2.font.ea}}"/>
                <a:cs typeface="{{master.content.level2.font.cs}}"/>
              </a:defRPr>
            </a:lvl2pPr>
            
            <!-- Level 3-5 similar structure... -->
          </a:lstStyle>
          
          <!-- Placeholder content -->
          <a:p>
            <a:pPr lvl="0"/>
            <a:r>
              <a:rPr lang="en-US" smtClean="0"/>
              <a:t>{{master.content.level1.placeholder}}</a:t>
            </a:r>
          </a:p>
          <a:p>
            <a:pPr lvl="1"/>
            <a:r>
              <a:rPr lang="en-US" smtClean="0"/>
              <a:t>{{master.content.level2.placeholder}}</a>
            </a:r>
          </a:p>
        </p:txBody>
      </p:sp>
      
      <!-- Footer Placeholder -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="{{master.footer.id}}" name="Footer Placeholder 3"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr>
            <p:ph type="ftr" sz="quarter"/>
          </p:nvPr>
        </p:nvSpPr>
        
        <p:spPr>
          <a:xfrm>
            <a:off x="{{master.footer.position.x}}" y="{{master.footer.position.y}}"/>
            <a:ext cx="{{master.footer.size.width}}" cy="{{master.footer.size.height}}"/>
          </a:xfrm>
          <a:prstGeom prst="rect">
            <a:avLst/>
          </a:prstGeom>
          <a:noFill/>
        </p:spPr>
        
        <!-- Footer text styling -->
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p>
            <a:endParaRPr sz="{{master.footer.font.size}}" lang="en-US">
              <a:solidFill>
                <a:schemeClr val="{{master.footer.color.scheme}}"/>
              </a:solidFill>
              <a:latin typeface="{{master.footer.font.family}}"/>
            </a:endParaRPr>
          </a:p>
        </p:txBody>
      </p:sp>
      
      <!-- Date Placeholder -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="{{master.date.id}}" name="Date Placeholder 4"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr>
            <p:ph type="dt" sz="quarter"/>
          </p:nvPr>
        </p:nvSpPr>
        
        <p:spPr>
          <a:xfrm>
            <a:off x="{{master.date.position.x}}" y="{{master.date.position.y}}"/>
            <a:ext cx="{{master.date.size.width}}" cy="{{master.date.size.height}}"/>
          </a:xfrm>
          <a:prstGeom prst="rect">
            <a:avLst/>
          </a:prstGeom>
          <a:noFill/>
        </p:spPr>
        
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p>
            <a:fld id="{{master.date.field.id}}" type="datetime1">
              <a:rPr sz="{{master.date.font.size}}" lang="en-US">
                <a:solidFill>
                  <a:schemeClr val="{{master.date.color.scheme}}"/>
                </a:solidFill>
                <a:latin typeface="{{master.date.font.family}}"/>
              </a:rPr>
              <a:t>{{master.date.placeholder}}</a:t>
            </a:fld>
            <a:endParaRPr sz="{{master.date.font.size}}" lang="en-US">
              <a:latin typeface="{{master.date.font.family}}"/>
            </a:endParaRPr>
          </a:p>
        </p:txBody>
      </p:sp>
      
      <!-- Slide Number Placeholder -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="{{master.slideNumber.id}}" name="Slide Number Placeholder 5"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr>
            <p:ph type="sldNum" sz="quarter"/>
          </p:nvPr>
        </p:nvSpPr>
        
        <p:spPr>
          <a:xfrm>
            <a:off x="{{master.slideNumber.position.x}}" y="{{master.slideNumber.position.y}}"/>
            <a:ext cx="{{master.slideNumber.size.width}}" cy="{{master.slideNumber.size.height}}"/>
          </a:xfrm>
          <a:prstGeom prst="rect">
            <a:avLst/>
          </a:prstGeom>
          <a:noFill/>
        </p:spPr>
        
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p>
            <a:fld id="{{master.slideNumber.field.id}}" type="slidenum">
              <a:rPr sz="{{master.slideNumber.font.size}}" lang="en-US">
                <a:solidFill>
                  <a:schemeClr val="{{master.slideNumber.color.scheme}}"/>
                </a:solidFill>
                <a:latin typeface="{{master.slideNumber.font.family}}"/>
              </a:rPr>
              <a:t>{{master.slideNumber.placeholder}}</a:t>
            </a:fld>
            <a:endParaRPr sz="{{master.slideNumber.font.size}}" lang="en-US">
              <a:latin typeface="{{master.slideNumber.font.family}}"/>
            </a:endParaRPr>
          </a:p>
        </p:txBody>
      </p:sp>
    </p:spTree>
  </p:cSld>
  
  <!-- Color map for theme colors -->
  <p:clrMap bg1="lt1" tx1="dk1" bg2="lt2" tx2="dk2" accent1="accent1" accent2="accent2" accent3="accent3" accent4="accent4" accent5="accent5" accent6="accent6" hlink="hlink" folHlink="folHlink"/>
  
  <!-- Slide layout references -->
  <p:sldLayoutIdLst>
    <p:sldLayoutId id="{{layout.title.id}}" r:id="rId1"/>
    <p:sldLayoutId id="{{layout.titleContent.id}}" r:id="rId2"/>
    <p:sldLayoutId id="{{layout.sectionHeader.id}}" r:id="rId3"/>
    <p:sldLayoutId id="{{layout.twoContent.id}}" r:id="rId4"/>
    <p:sldLayoutId id="{{layout.comparison.id}}" r:id="rId5"/>
    <p:sldLayoutId id="{{layout.titleOnly.id}}" r:id="rId6"/>
    <p:sldLayoutId id="{{layout.blank.id}}" r:id="rId7"/>
    <p:sldLayoutId id="{{layout.contentCaption.id}}" r:id="rId8"/>
    <p:sldLayoutId id="{{layout.pictureCaption.id}}" r:id="rId9"/>
  </p:sldLayoutIdLst>
  
</p:sldMaster>

<!-- SLIDE LAYOUT: Title Slide -->
<p:sldLayout xmlns:p="http://schemas.openxmlformats.org/presentationml/2006/main"
             xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main"
             type="title" preserve="1">
  
  <p:cSld name="Title Slide">
    <p:spTree>
      <p:nvGrpSpPr>
        <p:cNvPr id="1" name=""/>
        <p:cNvGrpSpPr/>
        <p:nvPr/>
      </p:nvGrpSpPr>
      
      <p:grpSpPr>
        <a:xfrm>
          <a:off x="0" y="0"/>
          <a:ext cx="0" cy="0"/>
          <a:chOff x="0" y="0"/>
          <a:chExt cx="0" cy="0"/>
        </a:xfrm>
      </p:grpSpPr>
      
      <!-- Title placeholder for title slide -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="2" name="Title 1"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr>
            <p:ph type="ctrTitle"/>
          </p:nvPr>
        </p:nvSpPr>
        
        <p:spPr>
          <a:xfrm>
            <a:off x="{{layout.title.title.position.x}}" y="{{layout.title.title.position.y}}"/>
            <a:ext cx="{{layout.title.title.size.width}}" cy="{{layout.title.title.size.height}}"/>
          </a:xfrm>
        </p:spPr>
        
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p>
            <a:r>
              <a:rPr lang="en-US" smtClean="0"/>
              <a:t>{{layout.title.title.placeholder}}</a:t>
            </a:r>
            <a:endParaRPr lang="en-US"/>
          </a:p>
        </p:txBody>
      </p:sp>
      
      <!-- Subtitle placeholder -->
      <p:sp>
        <p:nvSpPr>
          <p:cNvPr id="3" name="Subtitle 2"/>
          <p:cNvSpPr>
            <a:spLocks noGrp="1"/>
          </p:cNvSpPr>
          <p:nvPr>
            <p:ph type="subTitle" idx="1"/>
          </p:nvPr>
        </p:nvSpPr>
        
        <p:spPr>
          <a:xfrm>
            <a:off x="{{layout.title.subtitle.position.x}}" y="{{layout.title.subtitle.position.y}}"/>
            <a:ext cx="{{layout.title.subtitle.size.width}}" cy="{{layout.title.subtitle.size.height}}"/>
          </a:xfrm>
        </p:spPr>
        
        <p:txBody>
          <a:bodyPr/>
          <a:lstStyle/>
          <a:p>
            <a:r>
              <a:rPr lang="en-US" smtClean="0"/>
              <a:t>{{layout.title.subtitle.placeholder}}</a:t>
            </a:r>
            <a:endParaRPr lang="en-US"/>
          </a:p>
        </p:txBody>
      </p:sp>
    </p:spTree>
  </p:cSld>
  
</p:sldLayout>

<!-- SLIDE LAYOUT: Two Content -->
<p:sldLayout xmlns:p="http://schemas.openxmlformats.org/presentationml/2006/main"
             xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main"
             type="twoObj" preserve="1">
  
  <p:cSld name="Two Content">
    <p:spTree>
      <p:nvGrpSpPr>
        <p:cNvPr id="1" name=""/>
        <p:cNvGrpSpPr/>
        <p:nvPr/>
      </p:nvGrpSpPr>
      
      <p:grpSpPr>
        <a:xfrm>
