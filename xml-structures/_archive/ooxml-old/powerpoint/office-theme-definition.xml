<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- Office Theme Definition with Complete Design Token Integration -->
<!-- This provides the theme structure that all Office formats inherit from -->

<a:theme xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main" name="{{theme.name}}">
  
  <!-- Theme Elements - Core design system components -->
  <a:themeElements>
    
    <!-- Color Scheme - Design token controlled colors -->
    <a:clrScheme name="{{theme.colorScheme.name}}">
      
      <!-- Semantic colors controlled by design tokens -->
      <a:dk1>  <!-- Dark 1 - Primary text color -->
        <a:sysClr val="windowText" lastClr="{{color.text.primary}}"/>
      </a:dk1>
      
      <a:lt1>  <!-- Light 1 - Primary background color -->
        <a:sysClr val="window" lastClr="{{color.background.primary}}"/>
      </a:lt1>
      
      <a:dk2>  <!-- Dark 2 - Secondary text color -->
        <a:srgbClr val="{{color.text.secondary}}"/>
      </a:dk2>
      
      <a:lt2>  <!-- Light 2 - Secondary background color -->
        <a:srgbClr val="{{color.background.secondary}}"/>
      </a:lt2>
      
      <!-- Brand accent colors -->
      <a:accent1>  <!-- Primary brand color -->
        <a:srgbClr val="{{color.brand.primary}}"/>
      </a:accent1>
      
      <a:accent2>  <!-- Secondary brand color -->
        <a:srgbClr val="{{color.brand.secondary}}"/>
      </a:accent2>
      
      <a:accent3>  <!-- Success/positive color -->
        <a:srgbClr val="{{color.semantic.success}}"/>
      </a:accent3>
      
      <a:accent4>  <!-- Warning/caution color -->
        <a:srgbClr val="{{color.semantic.warning}}"/>
      </a:accent4>
      
      <a:accent5>  <!-- Error/danger color -->
        <a:srgbClr val="{{color.semantic.error}}"/>
      </a:accent5>
      
      <a:accent6>  <!-- Info/neutral color -->
        <a:srgbClr val="{{color.semantic.info}}"/>
      </a:accent6>
      
      <!-- Hyperlink colors -->
      <a:hlink>  <!-- Hyperlink color -->
        <a:srgbClr val="{{color.link.primary}}"/>
      </a:hlink>
      
      <a:folHlink>  <!-- Followed hyperlink color -->
        <a:srgbClr val="{{color.link.visited}}"/>
      </a:folHlink>
    </a:clrScheme>
    
    <!-- Font Scheme - Theme typography -->
    <a:fontScheme name="{{theme.fontScheme.name}}">
      
      <!-- Major Font - Used for headings -->
      <a:majorFont>
        <a:latin typeface="{{typography.font.theme.major}}" 
                 pitchFamily="{{typography.font.theme.major.pitchFamily}}" 
                 charset="{{typography.font.theme.major.charset}}"/>
        <a:ea typeface="{{typography.font.theme.major.eastAsian}}"/>
        <a:cs typeface="{{typography.font.theme.major.complexScript}}"/>
        
        <!-- Font fallbacks for different languages -->
        <a:font script="Arab" typeface="{{typography.font.arabic}}"/>
        <a:font script="Hebr" typeface="{{typography.font.hebrew}}"/>
        <a:font script="Thai" typeface="{{typography.font.thai}}"/>
        <a:font script="Ethi" typeface="{{typography.font.ethiopic}}"/>
        <a:font script="Beng" typeface="{{typography.font.bengali}}"/>
        <a:font script="Gujr" typeface="{{typography.font.gujarati}}"/>
        <a:font script="Khmr" typeface="{{typography.font.khmer}}"/>
        <a:font script="Knda" typeface="{{typography.font.kannada}}"/>
        <a:font script="Guru" typeface="{{typography.font.gurmukhi}}"/>
        <a:font script="Cans" typeface="{{typography.font.canadianAboriginal}}"/>
        <a:font script="Cher" typeface="{{typography.font.cherokee}}"/>
        <a:font script="Yiii" typeface="{{typography.font.yi}}"/>
        <a:font script="Tibt" typeface="{{typography.font.tibetan}}"/>
        <a:font script="Thaa" typeface="{{typography.font.thaana}}"/>
        <a:font script="Deva" typeface="{{typography.font.devanagari}}"/>
        <a:font script="Telu" typeface="{{typography.font.telugu}}"/>
        <a:font script="Taml" typeface="{{typography.font.tamil}}"/>
        <a:font script="Syrc" typeface="{{typography.font.syriac}}"/>
        <a:font script="Orya" typeface="{{typography.font.oriya}}"/>
        <a:font script="Mlym" typeface="{{typography.font.malayalam}}"/>
        <a:font script="Laoo" typeface="{{typography.font.lao}}"/>
        <a:font script="Sinh" typeface="{{typography.font.sinhala}}"/>
        <a:font script="Mong" typeface="{{typography.font.mongolian}}"/>
        <a:font script="Viet" typeface="{{typography.font.vietnamese}}"/>
        <a:font script="Uigh" typeface="{{typography.font.uighur}}"/>
        <a:font script="Geor" typeface="{{typography.font.georgian}}"/>
      </a:majorFont>
      
      <!-- Minor Font - Used for body text -->
      <a:minorFont>
        <a:latin typeface="{{typography.font.theme.minor}}" 
                 pitchFamily="{{typography.font.theme.minor.pitchFamily}}" 
                 charset="{{typography.font.theme.minor.charset}}"/>
        <a:ea typeface="{{typography.font.theme.minor.eastAsian}}"/>
        <a:cs typeface="{{typography.font.theme.minor.complexScript}}"/>
        
        <!-- Same language fallbacks as major font -->
        <a:font script="Arab" typeface="{{typography.font.arabic}}"/>
        <a:font script="Hebr" typeface="{{typography.font.hebrew}}"/>
        <a:font script="Thai" typeface="{{typography.font.thai}}"/>
        <a:font script="Ethi" typeface="{{typography.font.ethiopic}}"/>
        <a:font script="Beng" typeface="{{typography.font.bengali}}"/>
        <a:font script="Gujr" typeface="{{typography.font.gujarati}}"/>
        <a:font script="Khmr" typeface="{{typography.font.khmer}}"/>
        <a:font script="Knda" typeface="{{typography.font.kannada}}"/>
        <a:font script="Guru" typeface="{{typography.font.gurmukhi}}"/>
        <a:font script="Cans" typeface="{{typography.font.canadianAboriginal}}"/>
        <a:font script="Cher" typeface="{{typography.font.cherokee}}"/>
        <a:font script="Yiii" typeface="{{typography.font.yi}}"/>
        <a:font script="Tibt" typeface="{{typography.font.tibetan}}"/>
        <a:font script="Thaa" typeface="{{typography.font.thaana}}"/>
        <a:font script="Deva" typeface="{{typography.font.devanagari}}"/>
        <a:font script="Telu" typeface="{{typography.font.telugu}}"/>
        <a:font script="Taml" typeface="{{typography.font.tamil}}"/>
        <a:font script="Syrc" typeface="{{typography.font.syriac}}"/>
        <a:font script="Orya" typeface="{{typography.font.oriya}}"/>
        <a:font script="Mlym" typeface="{{typography.font.malayalam}}"/>
        <a:font script="Laoo" typeface="{{typography.font.lao}}"/>
        <a:font script="Sinh" typeface="{{typography.font.sinhala}}"/>
        <a:font script="Mong" typeface="{{typography.font.mongolian}}"/>
        <a:font script="Viet" typeface="{{typography.font.vietnamese}}"/>
        <a:font script="Uigh" typeface="{{typography.font.uighur}}"/>
        <a:font script="Geor" typeface="{{typography.font.georgian}}"/>
      </a:minorFont>
    </a:fontScheme>
    
    <!-- Format Scheme - Visual styling rules -->
    <a:fmtScheme name="{{theme.formatScheme.name}}">
      
      <!-- Fill Style List - Background fills -->
      <a:fillStyleLst>
        <!-- Subtle fill -->
        <a:solidFill>
          <a:schemeClr val="phClr"/>
        </a:solidFill>
        
        <!-- Moderate fill -->
        <a:gradFill rotWithShape="1">
          <a:gsLst>
            <a:gs pos="0">
              <a:schemeClr val="phClr">
                <a:lumMod val="{{theme.fill.moderate.start.luminance}}"/>
                <a:satMod val="{{theme.fill.moderate.start.saturation}}"/>
              </a:schemeClr>
            </a:gs>
            <a:gs pos="50000">
              <a:schemeClr val="phClr">
                <a:lumMod val="{{theme.fill.moderate.mid.luminance}}"/>
                <a:satMod val="{{theme.fill.moderate.mid.saturation}}"/>
              </a:schemeClr>
            </a:gs>
            <a:gs pos="100000">
              <a:schemeClr val="phClr">
                <a:lumMod val="{{theme.fill.moderate.end.luminance}}"/>
                <a:satMod val="{{theme.fill.moderate.end.saturation}}"/>
              </a:schemeClr>
            </a:gs>
          </a:gsLst>
          <a:lin ang="{{theme.fill.moderate.gradient.angle}}" scaled="1"/>
        </a:gradFill>
        
        <!-- Intense fill -->
        <a:gradFill rotWithShape="1">
          <a:gsLst>
            <a:gs pos="0">
              <a:schemeClr val="phClr">
                <a:lumMod val="{{theme.fill.intense.start.luminance}}"/>
                <a:satMod val="{{theme.fill.intense.start.saturation}}"/>
              </a:schemeClr>
            </a:gs>
            <a:gs pos="100000">
              <a:schemeClr val="phClr">
                <a:lumMod val="{{theme.fill.intense.end.luminance}}"/>
                <a:satMod val="{{theme.fill.intense.end.saturation}}"/>
              </a:schemeClr>
            </a:gs>
          </a:gsLst>
          <a:lin ang="{{theme.fill.intense.gradient.angle}}" scaled="1"/>
        </a:gradFill>
      </a:fillStyleLst>
      
      <!-- Line Style List - Border/outline styles -->
      <a:lnStyleLst>
        <!-- Subtle line -->
        <a:ln w="{{theme.line.subtle.width}}" cap="flat" cmpd="sng" algn="ctr">
          <a:solidFill>
            <a:schemeClr val="phClr">
              <a:shade val="{{theme.line.subtle.shade}}"/>
              <a:satMod val="{{theme.line.subtle.saturation}}"/>
            </a:schemeClr>
          </a:solidFill>
          <a:prstDash val="{{theme.line.subtle.dash}}"/>
          <a:miter lim="800000"/>
        </a:ln>
        
        <!-- Moderate line -->
        <a:ln w="{{theme.line.moderate.width}}" cap="flat" cmpd="sng" algn="ctr">
          <a:solidFill>
            <a:schemeClr val="phClr"/>
          </a:solidFill>
          <a:prstDash val="{{theme.line.moderate.dash}}"/>
          <a:miter lim="800000"/>
        </a:ln>
        
        <!-- Intense line -->
        <a:ln w="{{theme.line.intense.width}}" cap="flat" cmpd="sng" algn="ctr">
          <a:solidFill>
            <a:schemeClr val="phClr">
              <a:shade val="{{theme.line.intense.shade}}"/>
            </a:schemeClr>
          </a:solidFill>
          <a:prstDash val="{{theme.line.intense.dash}}"/>
          <a:miter lim="800000"/>
        </a:ln>
      </a:lnStyleLst>
      
      <!-- Effect Style List - Visual effects -->
      <a:effectStyleLst>
        <!-- Subtle effect -->
        <a:effectStyle>
          <a:effectLst>
            <a:outerShdw blurRad="{{theme.effect.subtle.shadow.blur}}" 
                         dist="{{theme.effect.subtle.shadow.distance}}" 
                         dir="{{theme.effect.subtle.shadow.angle}}" 
                         rotWithShape="0">
              <a:srgbClr val="{{theme.effect.subtle.shadow.color}}">
                <a:alpha val="{{theme.effect.subtle.shadow.alpha}}"/>
              </a:srgbClr>
            </a:outerShdw>
          </a:effectLst>
        </a:effectStyle>
        
        <!-- Moderate effect -->
        <a:effectStyle>
          <a:effectLst>
            <a:outerShdw blurRad="{{theme.effect.moderate.shadow.blur}}" 
                         dist="{{theme.effect.moderate.shadow.distance}}" 
                         dir="{{theme.effect.moderate.shadow.angle}}" 
                         rotWithShape="0">
              <a:srgbClr val="{{theme.effect.moderate.shadow.color}}">
                <a:alpha val="{{theme.effect.moderate.shadow.alpha}}"/>
              </a:srgbClr>
            </a:outerShdw>
          </a:effectLst>
        </a:effectStyle>
        
        <!-- Intense effect -->
        <a:effectStyle>
          <a:effectLst>
            <a:outerShdw blurRad="{{theme.effect.intense.shadow.blur}}" 
                         dist="{{theme.effect.intense.shadow.distance}}" 
                         dir="{{theme.effect.intense.shadow.angle}}" 
                         rotWithShape="0">
              <a:srgbClr val="{{theme.effect.intense.shadow.color}}">
                <a:alpha val="{{theme.effect.intense.shadow.alpha}}"/>
              </a:srgbClr>
            </a:outerShdw>
            
            <!-- Reflection for intense effect -->
            <a:reflection blurRad="{{theme.effect.intense.reflection.blur}}" 
                          stA="{{theme.effect.intense.reflection.startAlpha}}" 
                          endA="{{theme.effect.intense.reflection.endAlpha}}" 
                          endPos="{{theme.effect.intense.reflection.endPosition}}" 
                          dir="{{theme.effect.intense.reflection.direction}}" 
                          fadeDir="{{theme.effect.intense.reflection.fadeDirection}}" 
                          sx="100000" 
                          sy="-100000" 
                          kx="0" 
                          ky="0" 
                          algn="bl" 
                          rotWithShape="0"/>
          </a:effectLst>
          
          <!-- 3D Properties for intense effect -->
          <a:scene3d>
            <a:camera prst="{{theme.effect.intense.3d.camera.preset}}">
              <a:rot lat="{{theme.effect.intense.3d.camera.latitude}}" 
                     lon="{{theme.effect.intense.3d.camera.longitude}}" 
                     rev="{{theme.effect.intense.3d.camera.revolution}}"/>
            </a:camera>
            <a:lightRig rig="{{theme.effect.intense.3d.lighting.rig}}" 
                        dir="{{theme.effect.intense.3d.lighting.direction}}">
              <a:rot lat="{{theme.effect.intense.3d.lighting.latitude}}" 
                     lon="{{theme.effect.intense.3d.lighting.longitude}}" 
                     rev="{{theme.effect.intense.3d.lighting.revolution}}"/>
            </a:lightRig>
          </a:scene3d>
          
          <a:sp3d>
            <a:bevelT w="{{theme.effect.intense.3d.bevelTop.width}}" 
                      h="{{theme.effect.intense.3d.bevelTop.height}}"/>
            <a:bevelB w="{{theme.effect.intense.3d.bevelBottom.width}}" 
                      h="{{theme.effect.intense.3d.bevelBottom.height}}"/>
            <a:extrusionClr>
              <a:schemeClr val="phClr">
                <a:lumMod val="{{theme.effect.intense.3d.extrusion.luminance}}"/>
                <a:satMod val="{{theme.effect.intense.3d.extrusion.saturation}}"/>
              </a:schemeClr>
            </a:extrusionClr>
            <a:contourClr>
              <a:schemeClr val="phClr">
                <a:lumMod val="{{theme.effect.intense.3d.contour.luminance}}"/>
                <a:satMod val="{{theme.effect.intense.3d.contour.saturation}}"/>
              </a:schemeClr>
            </a:contourClr>
          </a:sp3d>
        </a:effectStyle>
      </a:effectStyleLst>
      
      <!-- Background Fill Style List -->
      <a:bgFillStyleLst>
        <!-- Subtle background -->
        <a:solidFill>
          <a:schemeClr val="phClr"/>
        </a:solidFill>
        
        <!-- Moderate background -->
        <a:gradFill rotWithShape="1">
          <a:gsLst>
            <a:gs pos="0">
              <a:schemeClr val="phClr">
                <a:tint val="{{theme.background.moderate.gradient.start.tint}}"/>
                <a:satMod val="{{theme.background.moderate.gradient.start.saturation}}"/>
              </a:schemeClr>
            </a:gs>
            <a:gs pos="40000">
              <a:schemeClr val="phClr">
                <a:tint val="{{theme.background.moderate.gradient.mid.tint}}"/>
                <a:satMod val="{{theme.background.moderate.gradient.mid.saturation}}"/>
              </a:schemeClr>
            </a:gs>
            <a:gs pos="100000">
              <a:schemeClr val="phClr">
                <a:shade val="{{theme.background.moderate.gradient.end.shade}}"/>
                <a:satMod val="{{theme.background.moderate.gradient.end.saturation}}"/>
              </a:schemeClr>
            </a:gs>
          </a:gsLst>
          <a:path path="circle">
            <a:fillToRect l="50000" t="50000" r="50000" b="50000"/>
          </a:path>
        </a:gradFill>
        
        <!-- Intense background -->
        <a:gradFill rotWithShape="1">
          <a:gsLst>
            <a:gs pos="0">
              <a:schemeClr val="phClr">
                <a:tint val="{{theme.background.intense.gradient.start.tint}}"/>
                <a:satMod val="{{theme.background.intense.gradient.start.saturation}}"/>
              </a:schemeClr>
            </a:gs>
            <a:gs pos="100000">
              <a:schemeClr val="phClr">
                <a:shade val="{{theme.background.intense.gradient.end.shade}}"/>
                <a:satMod val="{{theme.background.intense.gradient.end.saturation}}"/>
              </a:schemeClr>
            </a:gs>
          </a:gsLst>
          <a:path path="circle">
            <a:fillToRect l="50000" t="50000" r="50000" b="50000"/>
          </a:path>
        </a:gradFill>
      </a:bgFillStyleLst>
    </a:fmtScheme>
  </a:themeElements>
  
  <!-- Object Defaults - Default formatting for different object types -->
  <a:objectDefaults>
    <!-- Default shape properties -->
    <a:spDef>
      <a:spPr/>
      <a:bodyPr/>
      <a:lstStyle/>
      <a:style>
        <a:lnRef idx="{{theme.defaults.shape.line.index}}">
          <a:schemeClr val="{{theme.defaults.shape.line.scheme}}"/>
        </a:lnRef>
        <a:fillRef idx="{{theme.defaults.shape.fill.index}}">
          <a:schemeClr val="{{theme.defaults.shape.fill.scheme}}"/>
        </a:fillRef>
        <a:effectRef idx="{{theme.defaults.shape.effect.index}}">
          <a:schemeClr val="{{theme.defaults.shape.effect.scheme}}"/>
        </a:effectRef>
        <a:fontRef idx="{{theme.defaults.shape.font.index}}">
          <a:schemeClr val="{{theme.defaults.shape.font.scheme}}"/>
        </a:fontRef>
      </a:style>
    </a:spDef>
    
    <!-- Default line properties -->
    <a:lnDef>
      <a:spPr/>
      <a:bodyPr/>
      <a:lstStyle/>
      <a:style>
        <a:lnRef idx="{{theme.defaults.line.line.index}}">
          <a:schemeClr val="{{theme.defaults.line.line.scheme}}"/>
        </a:lnRef>
        <a:fillRef idx="{{theme.defaults.line.fill.index}}">
          <a:schemeClr val="{{theme.defaults.line.fill.scheme}}"/>
        </a:fillRef>
        <a:effectRef idx="{{theme.defaults.line.effect.index}}">
          <a:schemeClr val="{{theme.defaults.line.effect.scheme}}"/>
        </a:effectRef>
        <a:fontRef idx="{{theme.defaults.line.font.index}}">
          <a:schemeClr val="{{theme.defaults.line.font.scheme}}"/>
        </a:fontRef>
      </a:style>
    </a:lnDef>
    
    <!-- Default text properties -->
    <a:txDef>
      <a:spPr/>
      <a:bodyPr/>
      <a:lstStyle/>
      <a:style>
        <a:lnRef idx="{{theme.defaults.text.line.index}}">
          <a:schemeClr val="{{theme.defaults.text.line.scheme}}"/>
        </a:lnRef>
        <a:fillRef idx="{{theme.defaults.text.fill.index}}">
          <a:schemeClr val="{{theme.defaults.text.fill.scheme}}"/>
        </a:fillRef>
        <a:effectRef idx="{{theme.defaults.text.effect.index}}">
          <a:schemeClr val="{{theme.defaults.text.effect.scheme}}"/>
        </a:effectRef>
        <a:fontRef idx="{{theme.defaults.text.font.index}}">
          <a:schemeClr val="{{theme.defaults.text.font.scheme}}"/>
        </a:fontRef>
      </a:style>
    </a:txDef>
  </a:objectDefaults>
  
  <!-- Extra Color Scheme List - Additional color variations -->
  <a:extraClrSchemeLst/>
  
  <!-- Custom Color List - Organization-specific colors -->
  <a:custClrLst>
    <a:custClr name="{{theme.custom.color1.name}}">
      <a:srgbClr val="{{theme.custom.color1.value}}"/>
    </a:custClr>
    <a:custClr name="{{theme.custom.color2.name}}">
      <a:srgbClr val="{{theme.custom.color2.value}}"/>
    </a:custClr>
    <a:custClr name="{{theme.custom.color3.name}}">
      <a:srgbClr val="{{theme.custom.color3.value}}"/>
    </a:custClr>
  </a:custClrLst>
</a:theme>