  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="16" name="Subscription_Tier">
    <vt:lpwstr>{{organization.subscription.tier|Professional}}</vt:lpwstr>
  </property>

  <!-- Accessibility compliance -->
  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="17" name="WCAG_Compliance_Level">
    <vt:lpwstr>{{accessibility.wcag_level|AAA}}</vt:lpwstr>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="18" name="Color_Contrast_Verified">
    <vt:bool>{{accessibility.contrast_verified|true}}</vt:bool>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="19" name="Alt_Text_Complete">
