<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Properties xmlns="http://schemas.openxmlformats.org/officeDocument/2006/custom-properties"
           xmlns:vt="http://schemas.openxmlformats.org/officeDocument/2006/docPropsVTypes">

  <!-- StyleStack Design System Custom Properties for PowerPoint -->

  <!-- Design system metadata -->
  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="2" name="StyleStack_Version">
    <vt:lpwstr>{{stylestack.version|2025.1.0}}</vt:lpwstr>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="3" name="Design_System_Name">
    <vt:lpwstr>{{design_system.name|StyleStack Modern}}</vt:lpwstr>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="4" name="Corporate_Brand_Override">
    <vt:bool>{{design_system.corporate_override|true}}</vt:bool>
  </property>

  <!-- Brand identity properties -->
  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="5" name="Brand_Primary_Color">
    <vt:lpwstr>{{brand.colors.primary|#0EA5E9}}</vt:lpwstr>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="6" name="Brand_Secondary_Color">
    <vt:lpwstr>{{brand.colors.secondary|#8B5CF6}}</vt:lpwstr>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="7" name="Brand_Font_Family">
    <vt:lpwstr>{{typography.font.family|Inter}}</vt:lpwstr>
  </property>

  <!-- Presentation specific properties -->
  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="8" name="Presentation_Type">
    <vt:lpwstr>{{presentation.type|Corporate Presentation}}</vt:lpwstr>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="9" name="Slide_Dimensions">
    <vt:lpwstr>{{presentation.dimensions|16:9 Widescreen}}</vt:lpwstr>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="10" name="Design_Token_Count">
