  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="12" name="Auto_Update_Enabled">
    <vt:bool>{{template.auto_update|true}}</vt:bool>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="13" name="Master_Slide_Count">
    <vt:i4>{{presentation.slide_masters|1}}</vt:i4>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="14" name="Layout_Count">
    <vt:i4>{{presentation.slide_layouts|11}}</vt:i4>
