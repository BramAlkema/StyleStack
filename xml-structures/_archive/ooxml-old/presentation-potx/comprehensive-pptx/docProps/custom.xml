  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="23" name="Template_Generation_Version">
    <vt:lpwstr>{{system.generator_version|2025.1.0-beta}}</vt:lpwstr>
  </property>

  <property fmtid="{D5CDD505-2E9C-101B-9397-08002B2CF9AE}" pid="24" name="Last_Token_Sync">
    <vt:filetime>{{system.last_token_sync|2025-01-15T10:00:00Z}}</vt:filetime>
  </property>

</Properties>