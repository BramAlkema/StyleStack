<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Types xmlns="http://schemas.openxmlformats.org/package/2006/content-types">
  <!-- StyleStack Modern Theme Content Types with Design Token Integration -->

  <!-- Default extensions -->
  <Default Extension="rels" ContentType="application/vnd.openxmlformats-package.relationships+xml"/>
  <Default Extension="xml" ContentType="application/xml"/>
  <Default Extension="jpeg" ContentType="image/jpeg"/>
  <Default Extension="jpg" ContentType="image/jpeg"/>
  <Default Extension="png" ContentType="image/png"/>
  <Default Extension="gif" ContentType="image/gif"/>
  <Default Extension="emf" ContentType="image/x-emf"/>
  <Default Extension="wmf" ContentType="image/x-wmf"/>
  <Default Extension="svg" ContentType="image/svg+xml"/>

  <!-- Theme-specific content types -->
  <Override PartName="/theme/theme1.xml" ContentType="application/vnd.openxmlformats-officedocument.theme+xml"/>
  <Override PartName="/theme/themeManager.xml" ContentType="application/vnd.openxmlformats-officedocument.themeManager+xml"/>

  <!-- StyleStack design system assets -->
  <Override PartName="/theme/media/stylestack_logo.png" ContentType="image/png"/>
  <Override PartName="/theme/media/company_logo.png" ContentType="image/png"/>
  <Override PartName="/theme/media/brand_pattern.png" ContentType="image/png"/>
  <Override PartName="/theme/media/texture_bg.jpg" ContentType="image/jpeg"/>

  <!-- Font files for comprehensive typography -->
  <Override PartName="/theme/fonts/inter-regular.ttf" ContentType="application/font-sfnt"/>
  <Override PartName="/theme/fonts/inter-bold.ttf" ContentType="application/font-sfnt"/>
  <Override PartName="/theme/fonts/inter-medium.ttf" ContentType="application/font-sfnt"/>
</Types>