<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<worksheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main"
           xmlns:r="http://schemas.openxmlformats.org/officeDocument/2006/relationships"
           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
           mc:Ignorable="x14ac xr xr2 xr3">

  <!-- Sheet properties -->
  <sheetPr tabColor="rgb" codeName="StyleStack_Dashboard">
    <tabColor rgb="FF0EA5E9"/>
  </sheetPr>

  <!-- Sheet dimensions -->
  <dimension ref="A1:J20"/>

  <!-- Sheet views -->
  <sheetViews>
    <sheetView tabSelected="1" workbookViewId="0">
      <selection activeCell="A1" sqref="A1"/>
    </sheetView>
  </sheetViews>

  <!-- Sheet format properties -->
  <sheetFormatPr baseColWidth="10" defaultColWidth="12" defaultRowHeight="15" customHeight="1"/>

  <!-- Column definitions with design token variables -->
  <cols>
    <col min="1" max="1" width="20" customWidth="1" style="1"/>
    <col min="2" max="2" width="25" customWidth="1" style="2"/>
    <col min="3" max="10" width="15" customWidth="1" style="3"/>
  </cols>

  <!-- Sheet data with design tokens -->
  <sheetData>
    <!-- Header row with StyleStack branding -->
    <row r="1" spans="1:10" ht="30" customHeight="1" s="4">
      <c r="A1" s="5" t="inlineStr">
        <is>
          <t>{{document.title}}</t>
        </is>
      </c>
      <c r="B1" s="6" t="inlineStr">
        <is>
          <t>{{document.subtitle}}</t>
        </is>
      </c>
      <c r="C1" s="7">
        <!-- Company logo cell -->
      </c>
    </row>

    <!-- Design token showcase row -->
    <row r="2" spans="1:10" ht="25" customHeight="1">
      <c r="A2" s="8" t="inlineStr">
        <is>
          <t>Brand Primary</t>
        </is>
      </c>
      <c r="B2" s="9" t="inlineStr">
        <is>
          <t>{{brand.colors.primary}}</t>
        </is>
      </c>
      <c r="C2" s="10" t="inlineStr">
        <is>
          <t>Brand Secondary</t>
        </is>
      </c>
      <c r="D2" s="11" t="inlineStr">
        <is>
          <t>{{brand.colors.secondary}}</t>
        </is>
      </c>
    </row>

    <!-- Typography examples -->
    <row r="3" spans="1:10">
      <c r="A3" s="12" t="inlineStr">
        <is>
          <t>Typography</t>
        </is>
      </c>
      <c r="B3" s="13" t="inlineStr">
        <is>
          <t>{{typography.font.family}}</t>
        </is>
      </c>
    </row>

    <!-- Data table header -->
    <row r="5" spans="1:5" ht="20" customHeight="1" s="14">
      <c r="A5" s="15" t="inlineStr">
        <is>
          <t>{{table.headers.metric}}</t>
        </is>
      </c>
      <c r="B5" s="15" t="inlineStr">
        <is>
          <t>{{table.headers.value}}</t>
        </is>
      </c>
      <c r="C5" s="15" t="inlineStr">
        <is>
          <t>{{table.headers.target}}</t>
        </is>
      </c>
      <c r="D5" s="15" t="inlineStr">
        <is>
          <t>{{table.headers.status}}</t>
        </is>
      </c>
    </row>

    <!-- Data rows with conditional formatting -->
    <row r="6" spans="1:4">
      <c r="A6" s="16" t="inlineStr">
        <is>
          <t>{{data.row1.metric}}</t>
        </is>
      </c>
      <c r="B6" s="17">
        <v>{{data.row1.value}}</v>
      </c>
      <c r="C6" s="17">
        <v>{{data.row1.target}}</v>
      </c>
      <c r="D6" s="18" t="inlineStr">
        <is>
          <t>{{data.row1.status}}</t>
        </is>
      </c>
    </row>

    <row r="7" spans="1:4">
      <c r="A7" s="16" t="inlineStr">
        <is>
          <t>{{data.row2.metric}}</t>
        </is>
      </c>
      <c r="B7" s="17">
        <v>{{data.row2.value}}</v>
      </c>
      <c r="C7" s="17">
        <v>{{data.row2.target}}</v>
      </c>
      <c r="D7" s="19" t="inlineStr">
        <is>
          <t>{{data.row2.status}}</t>
        </is>
      </c>
    </row>

    <!-- Chart title -->
    <row r="10" spans="1:10">
      <c r="A10" s="20" t="inlineStr">
        <is>
          <t>{{chart.title}}</t>
        </is>
      </c>
    </row>
  </sheetData>

  <!-- Table definition -->
  <tableParts count="1">
    <tablePart r:id="rId1"/>
  </tableParts>

  <!-- Conditional formatting rules -->
  <conditionalFormatting sqref="D6:D20">
    <cfRule type="cellIs" dxfId="0" priority="1" operator="equal">
      <formula>"Complete"</formula>
    </cfRule>
    <cfRule type="cellIs" dxfId="1" priority="2" operator="equal">
      <formula>"In Progress"</formula>
    </cfRule>
    <cfRule type="cellIs" dxfId="2" priority="3" operator="equal">
      <formula>"At Risk"</formula>
    </cfRule>
  </conditionalFormatting>

  <!-- Data validations -->
  <dataValidations count="1">
    <dataValidation type="list" allowBlank="1" showInputMessage="1" showErrorMessage="1" sqref="D6:D20">
      <formula1>"Complete,In Progress,At Risk,Not Started"</formula1>
    </dataValidation>
  </dataValidations>

  <!-- Drawing reference (for charts) -->
  <drawing r:id="rId2"/>

  <!-- Page margins and setup -->
  <pageMargins left="0.7" right="0.7" top="0.75" bottom="0.75" header="0.3" footer="0.3"/>
  <pageSetup paperSize="9" orientation="portrait" useFirstPageNumber="1" fitToWidth="1" fitToHeight="1"/>

  <!-- Header footer -->
  <headerFooter differentOddEven="0" differentFirst="0">
    <oddHeader>&amp;C&amp;"Inter,Bold"&amp;14{{document.header}}</oddHeader>
    <oddFooter>&amp;L{{document.footer.left}}&amp;C{{document.footer.center}}&amp;R{{document.footer.right}}</oddFooter>
  </headerFooter>

</worksheet>