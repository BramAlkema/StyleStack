<?xml version="1.0" encoding="UTF-8"?>
<!-- SmartArt Definition with Complete OOXML Primitives -->
<!-- This provides SmartArt structures that design tokens can patch against -->

<!-- PowerPoint SmartArt Graphic -->
<p:graphicFrame xmlns:p="http://schemas.openxmlformats.org/presentationml/2006/main"
                xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main"
                xmlns:dgm="http://schemas.openxmlformats.org/drawingml/2006/diagram">
  
  <p:nvGraphicFramePr>
    <p:cNvPr id="{{smartart.id}}" name="{{smartart.name}}"/>
    <p:cNvGraphicFramePr>
      <a:graphicFrameLocks noGrp="1"/>
    </p:cNvGraphicFramePr>
    <p:nvPr>
      <p:extLst>
        <p:ext uri="{{smartart.extensions.uri}}">
          <p14:modId xmlns:p14="http://schemas.microsoft.com/office/powerpoint/2010/main" val="{{smartart.modId}}"/>
        </p:ext>
      </p:extLst>
    </p:nvPr>
  </p:nvGraphicFramePr>
  
  <p:xfrm>
    <a:off x="{{smartart.position.x}}" y="{{smartart.position.y}}"/>
    <a:ext cx="{{smartart.size.width}}" cy="{{smartart.size.height}}"/>
  </p:xfrm>
  
  <a:graphic>
    <a:graphicData uri="http://schemas.openxmlformats.org/drawingml/2006/diagram">
      
      <!-- SmartArt Data Model -->
      <dgm:relIds xmlns:dgm="http://schemas.openxmlformats.org/drawingml/2006/diagram" 
                  r:dm="{{smartart.data.relationId}}" 
                  r:lo="{{smartart.layout.relationId}}" 
                  r:qs="{{smartart.quickStyle.relationId}}" 
                  r:cs="{{smartart.colorStyle.relationId}}"
                  xmlns:r="http://schemas.openxmlformats.org/officeDocument/2006/relationships"/>
    </a:graphicData>
  </a:graphic>
</p:graphicFrame>

<!-- SmartArt Data Structure -->
<dgm:dataModel xmlns:dgm="http://schemas.openxmlformats.org/drawingml/2006/diagram">
  
  <!-- Point List - represents the hierarchy/structure -->
  <dgm:ptLst>
    <!-- Root node -->
    <dgm:pt modelId="{{smartart.root.modelId}}" type="node">
      <dgm:prSet>
        <dgm:pPr>
          <dgm:layout>
            <!-- Node layout properties controlled by design tokens -->
            <dgm:orgChart val="{{smartart.layout.orgChart}}"/>
            <dgm:chMax val="{{smartart.layout.childMax}}"/>
            <dgm:chPref val="{{smartart.layout.childPreferred}}"/>
          </dgm:layout>
          <dgm:shape>
            <!-- Shape properties for this node -->
            <dgm:adj idx="0" val="{{smartart.node.adjustment1}}"/>
            <dgm:adj idx="1" val="{{smartart.node.adjustment2}}"/>
          </dgm:shape>
        </dgm:pPr>
      </dgm:prSet>
      
      <!-- Node text content -->
      <dgm:t>
        <a:p>
          <a:pPr>
            <a:defRPr sz="{{smartart.text.font.size}}" b="{{smartart.text.font.bold}}">
              <a:solidFill>
                <a:schemeClr val="{{smartart.text.font.scheme}}">
                  <a:lumMod val="{{smartart.text.font.luminance}}"/>
                </a:schemeClr>
              </a:solidFill>
              <a:latin typeface="{{typography.font.body}}"/>
            </a:defRPr>
          </a:pPr>
          <a:r>
            <a:rPr lang="{{smartart.text.language}}"/>
            <a:t>{{content.smartart.root.text}}</a:t>
          </a:r>
        </a:p>
      </dgm:t>
    </dgm:pt>
    
    <!-- Child nodes -->
    <dgm:pt modelId="{{smartart.child1.modelId}}" type="node">
      <dgm:prSet>
        <dgm:pPr>
          <dgm:layout>
            <dgm:chMax val="{{smartart.child.layout.childMax}}"/>
            <dgm:chPref val="{{smartart.child.layout.childPreferred}}"/>
          </dgm:layout>
        </dgm:pPr>
      </dgm:prSet>
      <dgm:t>
        <a:p>
          <a:r>
            <a:rPr lang="{{smartart.text.language}}"/>
            <a:t>{{content.smartart.child1.text}}</a:t>
          </a:r>
        </a:p>
      </dgm:t>
    </dgm:pt>
    
    <dgm:pt modelId="{{smartart.child2.modelId}}" type="node">
      <dgm:t>
        <a:p>
          <a:r>
            <a:rPr lang="{{smartart.text.language}}"/>
            <a:t>{{content.smartart.child2.text}}</a:t>
          </a:r>
        </a:p>
      </dgm:t>
    </dgm:pt>
    
    <!-- Connection points -->
    <dgm:pt modelId="{{smartart.connection1.modelId}}" type="parTrans">
      <dgm:prSet>
        <dgm:pPr>
          <dgm:layout>
            <dgm:hierBranch val="{{smartart.connection.hierarchy}}"/>
          </dgm:layout>
        </dgm:pPr>
      </dgm:prSet>
    </dgm:pt>
  </dgm:ptLst>
  
  <!-- Connection List - defines relationships -->
  <dgm:cxnLst>
    <!-- Root to children connections -->
    <dgm:cxn modelId="{{smartart.connection1.modelId}}" 
             type="parOf" 
             srcId="{{smartart.root.modelId}}" 
             destId="{{smartart.child1.modelId}}" 
             srcOrd="0" 
             destOrd="0"/>
             
    <dgm:cxn modelId="{{smartart.connection2.modelId}}" 
             type="parOf" 
             srcId="{{smartart.root.modelId}}" 
             destId="{{smartart.child2.modelId}}" 
             srcOrd="1" 
             destOrd="0"/>
  </dgm:cxnLst>
  
  <!-- Background formatting -->
  <dgm:bg>
    <a:solidFill>
      <a:schemeClr val="{{smartart.background.scheme}}">
        <a:tint val="{{smartart.background.tint}}"/>
      </a:schemeClr>
    </a:solidFill>
    <a:effectLst>
      <a:outerShdw blurRad="{{smartart.background.shadow.blur}}" 
                   dist="{{smartart.background.shadow.distance}}" 
                   dir="{{smartart.background.shadow.angle}}">
        <a:prstClr val="{{smartart.background.shadow.color}}">
          <a:alpha val="{{smartart.background.shadow.alpha}}"/>
        </a:prstClr>
      </a:outerShdw>
    </a:effectLst>
  </dgm:bg>
  
  <!-- Whole diagram formatting -->
  <dgm:whole>
    <dgm:ln>
      <a:solidFill>
        <a:schemeClr val="{{smartart.border.scheme}}"/>
      </a:solidFill>
    </dgm:ln>
    <dgm:effectLst>
      <a:outerShdw blurRad="{{smartart.shadow.blur}}" 
                   dist="{{smartart.shadow.distance}}" 
                   dir="{{smartart.shadow.angle}}">
        <a:schemeClr val="{{smartart.shadow.scheme}}">
          <a:alpha val="{{smartart.shadow.alpha}}"/>
        </a:schemeClr>
      </a:outerShdw>
    </a:effectLst>
  </dgm:whole>
</dgm:dataModel>

<!-- SmartArt Layout Definition -->
<dgm:layoutDef xmlns:dgm="http://schemas.openxmlformats.org/drawingml/2006/diagram" 
               uniqueId="{{smartart.layout.uniqueId}}">
  
  <dgm:title lang="{{smartart.layout.title.language}}" val="{{smartart.layout.title.text}}"/>
  <dgm:desc lang="{{smartart.layout.description.language}}" val="{{smartart.layout.description.text}}"/>
  
  <!-- Category classifications -->
  <dgm:catLst>
    <dgm:cat type="{{smartart.category.type}}" pri="{{smartart.category.priority}}"/>
  </dgm:catLst>
  
  <!-- Sample data for preview -->
  <dgm:sampData>
    <dgm:dataModel>
      <dgm:ptLst>
        <dgm:pt modelId="0" type="doc">
          <dgm:t>
            <a:p>
              <a:r>
                <a:t>{{content.smartart.sample.root}}</a:t>
              </a:r>
            </a:p>
          </dgm:t>
        </dgm:pt>
        <dgm:pt modelId="1" type="node">
          <dgm:t>
            <a:p>
              <a:r>
                <a:t>{{content.smartart.sample.child1}}</a:t>
              </a:r>
            </a:p>
          </dgm:t>
        </dgm:pt>
        <dgm:pt modelId="2" type="node">
          <dgm:t>
            <a:p>
              <a:r>
                <a:t>{{content.smartart.sample.child2}}</a:t>
              </a:r>
            </a:p>
          </dgm:t>
        </dgm:pt>
      </dgm:ptLst>
      <dgm:cxnLst>
        <dgm:cxn modelId="3" type="parOf" srcId="0" destId="1"/>
        <dgm:cxn modelId="4" type="parOf" srcId="0" destId="2"/>
      </dgm:cxnLst>
    </dgm:dataModel>
  </dgm:sampData>
  
  <!-- Style data with design token integration -->
  <dgm:styleData>
    <dgm:dataModel>
      <dgm:ptLst>
        <dgm:pt modelId="0" type="node">
          <dgm:prSet>
            <dgm:style>
              <!-- Shape styles controlled by design tokens -->
              <a:lnRef idx="{{smartart.style.line.index}}">
                <a:schemeClr val="{{smartart.style.line.scheme}}"/>
              </a:lnRef>
              <a:fillRef idx="{{smartart.style.fill.index}}">
                <a:schemeClr val="{{smartart.style.fill.scheme}}"/>
              </a:fillRef>
              <a:effectRef idx="{{smartart.style.effect.index}}">
                <a:schemeClr val="{{smartart.style.effect.scheme}}"/>
              </a:effectRef>
              <a:fontRef idx="{{smartart.style.font.index}}">
                <a:schemeClr val="{{smartart.style.font.scheme}}"/>
              </a:fontRef>
            </dgm:style>
          </dgm:prSet>
        </dgm:pt>
      </dgm:ptLst>
    </dgm:dataModel>
  </dgm:styleData>
  
  <!-- Layout node that defines the algorithm -->
  <dgm:layoutNode name="{{smartart.layout.algorithm.name}}">
    <!-- Constraints for positioning -->
    <dgm:constrLst>
      <!-- Width constraint -->
      <dgm:constr type="w" for="ch" forName="node" 
                  op="gte" val="{{smartart.constraint.minWidth}}"/>
      <!-- Height constraint -->  
      <dgm:constr type="h" for="ch" forName="node" 
                  op="gte" val="{{smartart.constraint.minHeight}}"/>
      <!-- Spacing constraint -->
      <dgm:constr type="sp" for="ch" forName="node" 
                  op="equ" val="{{smartart.constraint.spacing}}"/>
    </dgm:constrLst>
    
    <!-- Rules for layout behavior -->
    <dgm:ruleLst>
      <dgm:rule type="{{smartart.rule.type}}" for="{{smartart.rule.for}}" 
                forName="{{smartart.rule.forName}}" ptType="{{smartart.rule.pointType}}"/>
    </dgm:ruleLst>
    
    <!-- Variable list for calculations -->
    <dgm:varLst>
      <dgm:animLvl val="{{smartart.animation.level}}"/>
      <dgm:animOne val="{{smartart.animation.one}}"/>
      <dgm:resizeHandles val="{{smartart.resize.handles}}"/>
    </dgm:varLst>
    
    <!-- For each algorithm for child nodes -->
    <dgm:forEach name="{{smartart.forEach.name}}" ref="{{smartart.forEach.ref}}">
      <dgm:layoutNode name="{{smartart.child.layout.name}}">
        
        <!-- Shape definition for child nodes -->
        <dgm:shape>
          <dgm:adjLst>
            <dgm:adj idx="0" val="{{smartart.child.shape.adj1}}"/>
            <dgm:adj idx="1" val="{{smartart.child.shape.adj2}}"/>
          </dgm:adjLst>
        </dgm:shape>
        
        <!-- Presentation of child node -->
        <dgm:presOf axis="{{smartart.child.presentation.axis}}" 
                    ptType="{{smartart.child.presentation.pointType}}">
          
          <!-- Constraints for child positioning -->
          <dgm:constrLst>
            <dgm:constr type="w" for="ch" op="equ" val="{{smartart.child.constraint.width}}"/>
            <dgm:constr type="h" for="ch" op="equ" val="{{smartart.child.constraint.height}}"/>
          </dgm:constrLst>
          
          <!-- Shape properties -->
          <dgm:spPr>
            <a:xfrm>
              <a:off x="{{smartart.child.position.x}}" y="{{smartart.child.position.y}}"/>
              <a:ext cx="{{smartart.child.size.width}}" cy="{{smartart.child.size.height}}"/>
            </a:xfrm>
            
            <a:prstGeom prst="{{smartart.child.geometry.preset}}">
              <a:avLst>
                <a:gd name="adj1" fmla="val {{smartart.child.geometry.adj1}}"/>
                <a:gd name="adj2" fmla="val {{smartart.child.geometry.adj2}}"/>
              </a:avLst>
            </a:prstGeom>
          </dgm:spPr>
          
          <!-- Text properties controlled by design tokens -->
          <dgm:txPr>
            <a:bodyPr anchor="{{smartart.child.text.anchor}}" 
                      anchorCtr="{{smartart.child.text.anchorCenter}}"/>
            <a:lstStyle>
              <a:lvl1pPr algn="{{smartart.child.text.alignment}}">
                <a:defRPr sz="{{smartart.child.text.font.size}}" 
                          b="{{smartart.child.text.font.bold}}" 
                          i="{{smartart.child.text.font.italic}}">
                  <a:solidFill>
                    <a:schemeClr val="{{smartart.child.text.font.scheme}}">
                      <a:lumMod val="{{smartart.child.text.font.luminance}}"/>
                    </a:schemeClr>
                  </a:solidFill>
                  <a:latin typeface="{{typography.font.body}}"/>
                </a:defRPr>
              </a:lvl1pPr>
            </a:lstStyle>
          </dgm:txPr>
        </dgm:presOf>
      </dgm:layoutNode>
    </dgm:forEach>
  </dgm:layoutNode>
</dgm:layoutDef>

<!-- SmartArt Quick Style Set -->
<dgm:styleDef xmlns:dgm="http://schemas.openxmlformats.org/drawingml/2006/diagram" 
              uniqueId="{{smartart.quickStyle.uniqueId}}">
  
  <dgm:title lang="{{smartart.quickStyle.title.language}}" val="{{smartart.quickStyle.title.text}}"/>
  <dgm:desc lang="{{smartart.quickStyle.description.language}}" val="{{smartart.quickStyle.description.text}}"/>
  
  <!-- Category for style organization -->
  <dgm:catLst>
    <dgm:cat type="{{smartart.quickStyle.category}}" pri="{{smartart.quickStyle.priority}}"/>
  </dgm:catLst>
  
  <!-- Scene 3D properties for entire diagram -->
  <dgm:scene3d>
    <a:camera prst="{{smartart.3d.camera.preset}}">
      <a:rot lat="{{smartart.3d.camera.latitude}}" 
             lon="{{smartart.3d.camera.longitude}}" 
             rev="{{smartart.3d.camera.revolution}}"/>
    </a:camera>
    <a:lightRig rig="{{smartart.3d.lighting.rig}}" dir="{{smartart.3d.lighting.direction}}">
      <a:rot lat="{{smartart.3d.lighting.latitude}}" 
             lon="{{smartart.3d.lighting.longitude}}" 
             rev="{{smartart.3d.lighting.revolution}}"/>
    </a:lightRig>
  </dgm:scene3d>
  
  <!-- Style data model for different node types -->
  <dgm:styleLbl name="{{smartart.style.node.name}}">
    
    <!-- Scene 3D for individual nodes -->
    <dgm:scene3d>
      <a:camera prst="{{smartart.style.node.3d.camera}}"/>
      <a:lightRig rig="{{smartart.style.node.3d.lighting}}"/>
    </dgm:scene3d>
    
    <!-- 3D shape properties -->
    <dgm:sp3d extrusionH="{{smartart.style.node.3d.extrusion}}" 
              contourW="{{smartart.style.node.3d.contour}}">
      <a:bevelT w="{{smartart.style.node.3d.bevelTop.width}}" 
                h="{{smartart.style.node.3d.bevelTop.height}}" 
                prst="{{smartart.style.node.3d.bevelTop.preset}}"/>
      <a:bevelB w="{{smartart.style.node.3d.bevelBottom.width}}" 
                h="{{smartart.style.node.3d.bevelBottom.height}}" 
                prst="{{smartart.style.node.3d.bevelBottom.preset}}"/>
      <a:extrusionClr>
        <a:schemeClr val="{{smartart.style.node.3d.extrusionColor}}">
          <a:lumMod val="{{smartart.style.node.3d.extrusionLum}}"/>
        </a:schemeClr>
      </a:extrusionClr>
      <a:contourClr>
        <a:schemeClr val="{{smartart.style.node.3d.contourColor}}">
          <a:lumMod val="{{smartart.style.node.3d.contourLum}}"/>
        </a:schemeClr>
      </a:contourClr>
    </dgm:sp3d>
    
    <!-- Text 3D properties -->
    <dgm:txPr>
      <a:sp3d>
        <a:bevelT w="{{smartart.style.text.3d.bevelWidth}}" 
                  h="{{smartart.style.text.3d.bevelHeight}}"/>
        <a:extrusionClr>
          <a:schemeClr val="{{smartart.style.text.3d.extrusionColor}}"/>
        </a:extrusionClr>
      </a:sp3d>
    </dgm:txPr>
    
    <!-- Style references that use design tokens -->
    <dgm:style>
      <a:lnRef idx="{{smartart.style.line.index}}">
        <a:schemeClr val="{{smartart.style.line.scheme}}">
          <a:lumMod val="{{smartart.style.line.luminance}}"/>
          <a:satMod val="{{smartart.style.line.saturation}}"/>
        </a:schemeClr>
      </a:lnRef>
      
      <a:fillRef idx="{{smartart.style.fill.index}}">
        <a:schemeClr val="{{smartart.style.fill.scheme}}">
          <a:lumMod val="{{smartart.style.fill.luminance}}"/>
          <a:satMod val="{{smartart.style.fill.saturation}}"/>
        </a:schemeClr>
      </a:fillRef>
      
      <a:effectRef idx="{{smartart.style.effect.index}}">
        <a:schemeClr val="{{smartart.style.effect.scheme}}">
          <a:lumMod val="{{smartart.style.effect.luminance}}"/>
        </a:schemeClr>
      </a:effectRef>
      
      <a:fontRef idx="{{smartart.style.font.index}}">
        <a:schemeClr val="{{smartart.style.font.scheme}}">
          <a:lumMod val="{{smartart.style.font.luminance}}"/>
        </a:schemeClr>
      </a:fontRef>
    </dgm:style>
  </dgm:styleLbl>
</dgm:styleDef>

<!-- SmartArt Color Style Set -->
<dgm:clrStyleDef xmlns:dgm="http://schemas.openxmlformats.org/drawingml/2006/diagram" 
                 uniqueId="{{smartart.colorStyle.uniqueId}}">
  
  <dgm:title lang="{{smartart.colorStyle.title.language}}" val="{{smartart.colorStyle.title.text}}"/>
  <dgm:desc lang="{{smartart.colorStyle.description.language}}" val="{{smartart.colorStyle.description.text}}"/>
  
  <!-- Color categories -->
  <dgm:catLst>
    <dgm:cat type="{{smartart.colorStyle.category}}" pri="{{smartart.colorStyle.priority}}"/>
  </dgm:catLst>
  
  <!-- Variation list for different color combinations -->
  <dgm:variation>
    <!-- Background fill -->
    <dgm:fillClrLst meth="{{smartart.color.method}}" hueDir="{{smartart.color.hueDirection}}">
      <a:schemeClr val="{{color.brand.primary}}"/>
      <a:schemeClr val="{{color.brand.secondary}}"/>
      <a:schemeClr val="{{color.semantic.success}}"/>
      <a:schemeClr val="{{color.semantic.warning}}"/>
      <a:schemeClr val="{{color.semantic.error}}"/>
      <a:schemeClr val="{{color.semantic.info}}"/>
    </dgm:fillClrLst>
    
    <!-- Line colors -->
    <dgm:linClrLst meth="{{smartart.color.line.method}}" hueDir="{{smartart.color.line.hueDirection}}">
      <a:schemeClr val="{{smartart.color.line.primary}}"/>
      <a:schemeClr val="{{smartart.color.line.secondary}}"/>
      <a:schemeClr val="{{smartart.color.line.accent}}"/>
    </dgm:linClrLst>
    
    <!-- Effect colors -->
    <dgm:effectClrLst meth="{{smartart.color.effect.method}}" hueDir="{{smartart.color.effect.hueDirection}}">
      <a:schemeClr val="{{smartart.color.effect.primary}}"/>
      <a:schemeClr val="{{smartart.color.effect.secondary}}"/>
    </dgm:effectClrLst>
    
    <!-- Text colors -->
    <dgm:txFillClrLst meth="{{smartart.color.text.method}}" hueDir="{{smartart.color.text.hueDirection}}">
      <a:schemeClr val="{{color.text.primary}}"/>
      <a:schemeClr val="{{color.text.secondary}}"/>
      <a:schemeClr val="{{color.text.inverse}}"/>
    </dgm:txFillClrLst>
    
    <!-- Text line colors -->
    <dgm:txLinClrLst meth="{{smartart.color.textLine.method}}">
      <a:schemeClr val="{{smartart.color.textLine.primary}}"/>
      <a:schemeClr val="{{smartart.color.textLine.secondary}}"/>
    </dgm:txLinClrLst>
    
    <!-- Text effect colors -->
    <dgm:txEffectClrLst meth="{{smartart.color.textEffect.method}}">
      <a:schemeClr val="{{smartart.color.textEffect.primary}}"/>
      <a:schemeClr val="{{smartart.color.textEffect.secondary}}"/>
    </dgm:txEffectClrLst>
  </dgm:variation>
</dgm:clrStyleDef>

<!-- Common SmartArt Layout Types with Design Token Integration -->
<!-- 
  Process Layouts:
  - Basic Process: urn:microsoft.com/office/officeart/2005/8/layout/default
  - Alternating Flow: urn:microsoft.com/office/officeart/2005/8/layout/alternatingFlow
  - Continuous Block Process: urn:microsoft.com/office/officeart/2005/8/layout/continuousBlockProcess
  
  Hierarchy Layouts:  
  - Organization Chart: urn:microsoft.com/office/officeart/2005/8/layout/orgChart
  - Hierarchy List: urn:microsoft.com/office/officeart/2005/8/layout/hierList
  
  Cycle Layouts:
  - Basic Cycle: urn:microsoft.com/office/officeart/2005/8/layout/cycle
  - Radial Cycle: urn:microsoft.com/office/officeart/2005/8/layout/radialCycle
  
  Relationship Layouts:
  - Basic Venn: urn:microsoft.com/office/officeart/2005/8/layout/venn
  - Radial Venn: urn:microsoft.com/office/officeart/2005/8/layout/radialVenn
  
  Matrix Layouts:
  - Basic Matrix: urn:microsoft.com/office/officeart/2005/8/layout/matrix
  - Grid Matrix: urn:microsoft.com/office/officeart/2005/8/layout/gridMatrix
  
  Pyramid Layouts:
  - Basic Pyramid: urn:microsoft.com/office/officeart/2005/8/layout/pyramid
  - Inverted Pyramid: urn:microsoft.com/office/officeart/2005/8/layout/invertedPyramid
-->