<?xml version="1.0" encoding="UTF-8"?>
<!-- Office Shape Layout System with Complete OOXML Primitives -->
<!-- This provides the layout arrangements and alignment systems that design tokens can patch against -->

<!-- PowerPoint Shape Layout Container -->
<p:spTree xmlns:p="http://schemas.openxmlformats.org/presentationml/2006/main"
          xmlns:a="http://schemas.openxmlformats.org/drawingml/2006/main">
  
  <!-- Group Shape Properties for Layout Container -->
  <p:nvGrpSpPr>
    <p:cNvPr id="{{layout.container.id}}" name="{{layout.container.name}}"/>
    <p:cNvGrpSpPr/>
    <p:nvPr/>
  </p:nvGrpSpPr>
  
  <!-- Group Shape Properties -->
  <p:grpSpPr>
    <!-- Transform for entire layout group -->
    <a:xfrm>
      <a:off x="{{layout.container.position.x}}" y="{{layout.container.position.y}}"/>
      <a:ext cx="{{layout.container.size.width}}" cy="{{layout.container.size.height}}"/>
      <a:chOff x="{{layout.container.child.offset.x}}" y="{{layout.container.child.offset.y}}"/>
      <a:chExt cx="{{layout.container.child.extent.width}}" cy="{{layout.container.child.extent.height}}"/>
    </a:xfrm>
  </p:grpSpPr>
  
  <!-- LAYOUT TYPE: Horizontal Flow -->
  <!-- Shapes arranged horizontally with consistent spacing -->
  <LayoutGroup name="HorizontalFlow" type="linear">
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.horizontal.shape1.id}}" name="{{layout.horizontal.shape1.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <!-- First shape position controlled by design tokens -->
          <a:off x="{{layout.horizontal.shape1.x}}" y="{{layout.horizontal.baseline.y}}"/>
          <a:ext cx="{{layout.horizontal.shape.width}}" cy="{{layout.horizontal.shape.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.horizontal.shape.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.horizontal.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.horizontal.shape2.id}}" name="{{layout.horizontal.shape2.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <!-- Second shape with calculated spacing -->
          <a:off x="{{layout.horizontal.shape2.x}}" y="{{layout.horizontal.baseline.y}}"/>
          <a:ext cx="{{layout.horizontal.shape.width}}" cy="{{layout.horizontal.shape.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.horizontal.shape.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.horizontal.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <!-- Connector between horizontal shapes -->
    <p:cxnSp>
      <p:nvCxnSpPr>
        <p:cNvPr id="{{layout.horizontal.connector.id}}" name="{{layout.horizontal.connector.name}}"/>
        <p:cNvCxnSpPr>
          <a:cxnSpLocks/>
        </p:cNvCxnSpPr>
        <p:nvPr/>
      </p:nvCxnSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.horizontal.connector.x}}" y="{{layout.horizontal.connector.y}}"/>
          <a:ext cx="{{layout.horizontal.connector.width}}" cy="{{layout.horizontal.connector.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.horizontal.connector.geometry}}"/>
        <a:ln w="{{layout.horizontal.connector.line.width}}">
          <a:solidFill>
            <a:schemeClr val="{{layout.horizontal.connector.color.scheme}}"/>
          </a:solidFill>
          <a:headEnd type="{{layout.horizontal.connector.arrow.head}}"/>
          <a:tailEnd type="{{layout.horizontal.connector.arrow.tail}}"/>
        </a:ln>
      </p:spPr>
    </p:cxnSp>
  </LayoutGroup>
  
  <!-- LAYOUT TYPE: Vertical Stack -->
  <!-- Shapes arranged vertically with consistent alignment -->
  <LayoutGroup name="VerticalStack" type="stack">
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.vertical.shape1.id}}" name="{{layout.vertical.shape1.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <!-- Top shape in vertical stack -->
          <a:off x="{{layout.vertical.baseline.x}}" y="{{layout.vertical.shape1.y}}"/>
          <a:ext cx="{{layout.vertical.shape.width}}" cy="{{layout.vertical.shape.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.vertical.shape.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.vertical.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.vertical.shape2.id}}" name="{{layout.vertical.shape2.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <!-- Second shape with calculated vertical spacing -->
          <a:off x="{{layout.vertical.baseline.x}}" y="{{layout.vertical.shape2.y}}"/>
          <a:ext cx="{{layout.vertical.shape.width}}" cy="{{layout.vertical.shape.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.vertical.shape.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.vertical.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
  </LayoutGroup>
  
  <!-- LAYOUT TYPE: Grid Layout -->
  <!-- 2x2 Grid arrangement with consistent spacing -->
  <LayoutGroup name="GridLayout" type="grid">
    <!-- Row 1, Column 1 -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.grid.r1c1.id}}" name="{{layout.grid.r1c1.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.grid.col1.x}}" y="{{layout.grid.row1.y}}"/>
          <a:ext cx="{{layout.grid.cell.width}}" cy="{{layout.grid.cell.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.grid.shape.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.grid.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <!-- Row 1, Column 2 -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.grid.r1c2.id}}" name="{{layout.grid.r1c2.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.grid.col2.x}}" y="{{layout.grid.row1.y}}"/>
          <a:ext cx="{{layout.grid.cell.width}}" cy="{{layout.grid.cell.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.grid.shape.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.grid.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <!-- Row 2, Column 1 -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.grid.r2c1.id}}" name="{{layout.grid.r2c1.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.grid.col1.x}}" y="{{layout.grid.row2.y}}"/>
          <a:ext cx="{{layout.grid.cell.width}}" cy="{{layout.grid.cell.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.grid.shape.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.grid.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <!-- Row 2, Column 2 -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.grid.r2c2.id}}" name="{{layout.grid.r2c2.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.grid.col2.x}}" y="{{layout.grid.row2.y}}"/>
          <a:ext cx="{{layout.grid.cell.width}}" cy="{{layout.grid.cell.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.grid.shape.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.grid.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
  </LayoutGroup>
  
  <!-- LAYOUT TYPE: Radial/Circular Layout -->
  <!-- Shapes arranged in a circle around center point -->
  <LayoutGroup name="RadialLayout" type="radial">
    <!-- Center shape -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.radial.center.id}}" name="{{layout.radial.center.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.radial.center.x}}" y="{{layout.radial.center.y}}"/>
          <a:ext cx="{{layout.radial.center.width}}" cy="{{layout.radial.center.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.radial.center.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.radial.center.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <!-- Satellite shapes positioned around center -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.radial.satellite1.id}}" name="{{layout.radial.satellite1.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <!-- Position calculated using radius and angle -->
          <a:off x="{{layout.radial.satellite1.x}}" y="{{layout.radial.satellite1.y}}"/>
          <a:ext cx="{{layout.radial.satellite.width}}" cy="{{layout.radial.satellite.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.radial.satellite.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.radial.satellite.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <!-- Additional satellites at calculated angles -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.radial.satellite2.id}}" name="{{layout.radial.satellite2.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.radial.satellite2.x}}" y="{{layout.radial.satellite2.y}}"/>
          <a:ext cx="{{layout.radial.satellite.width}}" cy="{{layout.radial.satellite.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.radial.satellite.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.radial.satellite.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
  </LayoutGroup>
  
  <!-- LAYOUT TYPE: Hierarchy Tree -->
  <!-- Tree structure with parent-child relationships -->
  <LayoutGroup name="HierarchyTree" type="tree">
    <!-- Root/Parent node -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.hierarchy.root.id}}" name="{{layout.hierarchy.root.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.hierarchy.root.x}}" y="{{layout.hierarchy.root.y}}"/>
          <a:ext cx="{{layout.hierarchy.root.width}}" cy="{{layout.hierarchy.root.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.hierarchy.root.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.hierarchy.root.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <!-- Child nodes positioned below parent -->
    <p:sp>
      <p:nvSpPr>
        <p:cNvPr id="{{layout.hierarchy.child1.id}}" name="{{layout.hierarchy.child1.name}}"/>
        <p:cNvSpPr/>
        <p:nvPr/>
      </p:nvSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.hierarchy.child1.x}}" y="{{layout.hierarchy.child1.y}}"/>
          <a:ext cx="{{layout.hierarchy.child.width}}" cy="{{layout.hierarchy.child.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.hierarchy.child.geometry}}"/>
        <a:solidFill>
          <a:schemeClr val="{{layout.hierarchy.child.fill.scheme}}"/>
        </a:solidFill>
      </p:spPr>
    </p:sp>
    
    <!-- Connecting lines between parent and children -->
    <p:cxnSp>
      <p:nvCxnSpPr>
        <p:cNvPr id="{{layout.hierarchy.connector1.id}}" name="{{layout.hierarchy.connector1.name}}"/>
        <p:cNvCxnSpPr>
          <a:cxnSpLocks/>
        </p:cNvCxnSpPr>
        <p:nvPr/>
      </p:nvCxnSpPr>
      <p:spPr>
        <a:xfrm>
          <a:off x="{{layout.hierarchy.connector1.x}}" y="{{layout.hierarchy.connector1.y}}"/>
          <a:ext cx="{{layout.hierarchy.connector1.width}}" cy="{{layout.hierarchy.connector1.height}}"/>
        </a:xfrm>
        <a:prstGeom prst="{{layout.hierarchy.connector.geometry}}"/>
        <a:ln w="{{layout.hierarchy.connector.line.width}}">
          <a:solidFill>
            <a:schemeClr val="{{layout.hierarchy.connector.color.scheme}}"/>
          </a:solidFill>
        </a:ln>
      </p:spPr>
    </p:cxnSp>
  </LayoutGroup>
</p:spTree>

<!-- Layout Alignment Options -->
<LayoutAlignment>
  <!-- Horizontal Alignment Options -->
  <HorizontalAlign>
    <Left value="{{layout.align.horizontal.left}}"/>
    <Center value="{{layout.align.horizontal.center}}"/>
    <Right value="{{layout.align.horizontal.right}}"/>
    <Distribute value="{{layout.align.horizontal.distribute}}"/>
  </HorizontalAlign>
  
  <!-- Vertical Alignment Options -->
  <VerticalAlign>
    <Top value="{{layout.align.vertical.top}}"/>
    <Middle value="{{layout.align.vertical.middle}}"/>
    <Bottom value="{{layout.align.vertical.bottom}}"/>
    <Distribute value="{{layout.align.vertical.distribute}}"/>
  </VerticalAlign>
  
  <!-- Distribution Options -->
  <Distribution>
    <HorizontalSpacing value="{{layout.distribute.horizontal.spacing}}"/>
    <VerticalSpacing value="{{layout.distribute.vertical.spacing}}"/>
    <EqualSpacing value="{{layout.distribute.equal.spacing}}"/>
  </Distribution>
</LayoutAlignment>

<!-- Layout Spacing and Margins -->
<LayoutSpacing>
  <!-- Shape-to-Shape Spacing -->
  <ShapeSpacing>
    <Horizontal value="{{layout.spacing.shape.horizontal}}"/>
    <Vertical value="{{layout.spacing.shape.vertical}}"/>
    <Minimum value="{{layout.spacing.shape.minimum}}"/>
    <Preferred value="{{layout.spacing.shape.preferred}}"/>
  </ShapeSpacing>
  
  <!-- Container Margins -->
  <ContainerMargins>
    <Top value="{{layout.margins.container.top}}"/>
    <Right value="{{layout.margins.container.right}}"/>
    <Bottom value="{{layout.margins.container.bottom}}"/>
    <Left value="{{layout.margins.container.left}}"/>
  </ContainerMargins>
  
  <!-- Grid Spacing -->
  <GridSpacing>
    <RowGap value="{{layout.grid.row.gap}}"/>
    <ColumnGap value="{{layout.grid.column.gap}}"/>
    <CellPadding value="{{layout.grid.cell.padding}}"/>
  </GridSpacing>
</LayoutSpacing>

<!-- Layout Responsive Behavior -->
<LayoutResponsive>
  <!-- Resize Behavior -->
  <ResizeBehavior>
    <MaintainAspectRatio value="{{layout.resize.maintainAspect}}"/>
    <ScaleWithContainer value="{{layout.resize.scaleWithContainer}}"/>
    <SnapToGrid value="{{layout.resize.snapToGrid}}"/>
    <MinimumSize width="{{layout.resize.minimumWidth}}" height="{{layout.resize.minimumHeight}}"/>
  </ResizeBehavior>
  
  <!-- Reflow Options -->
  <Reflow>
    <WrapToNewLine value="{{layout.reflow.wrap}}"/>
    <OverflowBehavior value="{{layout.reflow.overflow}}"/>
    <Priority value="{{layout.reflow.priority}}"/>
  </Reflow>
</LayoutResponsive>

<!-- Quick Layout Presets -->
<QuickLayoutPresets>
  <!-- Process Flow Preset -->
  <Preset name="ProcessFlow" category="process">
    <Description>Horizontal flow with arrows between shapes</Description>
    <ShapeCount min="2" max="8" default="4"/>
    <ShapeType>{{layout.preset.process.shapeType}}</ShapeType>
    <ConnectorType>{{layout.preset.process.connectorType}}</ConnectorType>
    <Spacing>{{layout.preset.process.spacing}}</Spacing>
    <Colors>{{layout.preset.process.colorScheme}}</Colors>
  </Preset>
  
  <!-- Organization Chart Preset -->
  <Preset name="OrgChart" category="hierarchy">
    <Description>Hierarchical organization structure</Description>
    <ShapeCount min="3" max="15" default="7"/>
    <ShapeType>{{layout.preset.orgchart.shapeType}}</ShapeType>
    <ConnectorType>{{layout.preset.orgchart.connectorType}}</ConnectorType>
    <Levels>{{layout.preset.orgchart.levels}}</Levels>
    <Colors>{{layout.preset.orgchart.colorScheme}}</Colors>
  </Preset>
  
  <!-- Feature Comparison Preset -->
  <Preset name="Comparison" category="comparison">
    <Description>Side-by-side comparison layout</Description>
    <ShapeCount min="2" max="6" default="3"/>
    <ShapeType>{{layout.preset.comparison.shapeType}}</ShapeType>
    <Columns>{{layout.preset.comparison.columns}}</Columns>
    <HeaderRow>{{layout.preset.comparison.headerRow}}</HeaderRow>
    <Colors>{{layout.preset.comparison.colorScheme}}</Colors>
  </Preset>
  
  <!-- Timeline Preset -->
  <Preset name="Timeline" category="process">
    <Description>Chronological sequence of events</Description>
    <ShapeCount min="3" max="10" default="5"/>
    <ShapeType>{{layout.preset.timeline.shapeType}}</ShapeType>
    <ConnectorType>{{layout.preset.timeline.connectorType}}</ConnectorType>
    <Orientation>{{layout.preset.timeline.orientation}}</Orientation>
    <Colors>{{layout.preset.timeline.colorScheme}}</Colors>
  </Preset>
</QuickLayoutPresets>

<!-- Layout Animation and Transitions -->
<LayoutAnimation>
  <!-- Entrance Effects for Layout Groups -->
  <EntranceEffect>
    <FadeIn duration="{{layout.animation.fadeIn.duration}}" delay="{{layout.animation.fadeIn.delay}}"/>
    <SlideIn direction="{{layout.animation.slideIn.direction}}" duration="{{layout.animation.slideIn.duration}}"/>
    <ScaleIn scale="{{layout.animation.scaleIn.scale}}" duration="{{layout.animation.scaleIn.duration}}"/>
  </EntranceEffect>
  
  <!-- Sequential Animation for Shape Groups -->
  <SequentialAnimation>
    <DelayBetweenShapes value="{{layout.animation.sequential.delay}}"/>
    <AnimationOrder value="{{layout.animation.sequential.order}}"/>
    <GroupTogether value="{{layout.animation.sequential.group}}"/>
  </SequentialAnimation>
</LayoutAnimation>

<!-- Design Token Integration for Layout System -->
<LayoutDesignTokens>
  <!-- Spacing System -->
  <SpacingTokens>
    <XS>{{spacing.xs}}</XS>          <!-- 4pt -->
    <SM>{{spacing.sm}}</SM>          <!-- 8pt -->
    <MD>{{spacing.md}}</MD>          <!-- 16pt -->
    <LG>{{spacing.lg}}</LG>          <!-- 24pt -->
    <XL>{{spacing.xl}}</XL>          <!-- 32pt -->
    <XXL>{{spacing.xxl}}</XXL>       <!-- 48pt -->
  </SpacingTokens>
  
  <!-- Layout Grid System -->
  <GridTokens>
    <Columns>{{grid.columns}}</Columns>
    <Gutter>{{grid.gutter}}</Gutter>
    <Margin>{{grid.margin}}</Margin>
    <MaxWidth>{{grid.maxWidth}}</MaxWidth>
  </GridTokens>
  
  <!-- Breakpoints for Responsive Layouts -->
  <Breakpoints>
    <Mobile>{{breakpoint.mobile}}</Mobile>     <!-- 480px -->
    <Tablet>{{breakpoint.tablet}}</Tablet>     <!-- 768px -->
    <Desktop>{{breakpoint.desktop}}</Desktop>   <!-- 1024px -->
    <Wide>{{breakpoint.wide}}</Wide>           <!-- 1440px -->
  </Breakpoints>
</LayoutDesignTokens>