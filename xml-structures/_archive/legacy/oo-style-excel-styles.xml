<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<styleSheet xmlns="http://schemas.openxmlformats.org/spreadsheetml/2006/main">
  <!-- Font definitions inspired by LibreOffice defaults -->
  <fonts count="6">
    <!-- Default: Liberation Sans -->
    <font>
      <sz val="10"/>
      <name val="Liberation Sans"/>
      <family val="2"/>
    </font>
    <!-- Bold -->
    <font>
      <sz val="10"/>
      <name val="Liberation Sans"/>
      <family val="2"/>
      <b/>
    </font>
    <!-- Heading -->
    <font>
      <sz val="18"/>
      <name val="Liberation Sans"/>
      <family val="2"/>
      <b/>
    </font>
    <!-- Subheading -->
    <font>
      <sz val="12"/>
      <name val="Liberation Sans"/>
      <family val="2"/>
      <b/>
    </font>
    <!-- Status - Good -->
    <font>
      <sz val="10"/>
      <name val="Liberation Sans"/>
      <family val="2"/>
      <color rgb="FF006600"/>
    </font>
    <!-- Status - Error -->
    <font>
      <sz val="10"/>
      <name val="Liberation Sans"/>
      <family val="2"/>
      <b/>
      <color rgb="FFFFFFFF"/>
    </font>
  </fonts>
  
  <!-- Fill patterns inspired by LibreOffice status styles -->
  <fills count="8">
    <fill><patternFill patternType="none"/></fill>
    <fill><patternFill patternType="gray125"/></fill>
    <!-- Good status -->
    <fill><patternFill patternType="solid"><fgColor rgb="FFCCFFCC"/></patternFill></fill>
    <!-- Warning status -->
    <fill><patternFill patternType="solid"><fgColor rgb="FFFFFFCC"/></patternFill></fill>
    <!-- Error status -->
    <fill><patternFill patternType="solid"><fgColor rgb="FFCC0000"/></patternFill></fill>
    <!-- Accent 1 -->
    <fill><patternFill patternType="solid"><fgColor rgb="FF000000"/></patternFill></fill>
    <!-- Accent 2 -->
    <fill><patternFill patternType="solid"><fgColor rgb="FF808080"/></patternFill></fill>
    <!-- Accent 3 -->
    <fill><patternFill patternType="solid"><fgColor rgb="FFDDDDDD"/></patternFill></fill>
  </fills>
  
  <!-- Simple borders -->
  <borders count="2">
    <border/>
    <border>
      <left style="thin"/>
      <right style="thin"/>
      <top style="thin"/>
      <bottom style="thin"/>
    </border>
  </borders>
  
  <!-- Cell style formats -->
  <cellStyleXfs count="1">
    <xf numFmtId="0" fontId="0" fillId="0" borderId="0" 
        applyFont="true" applyBorder="true" applyAlignment="true">
      <alignment horizontal="general" vertical="bottom"/>
    </xf>
  </cellStyleXfs>
  
  <!-- Cell formats matching LibreOffice styles -->
  <cellXfs count="8">
    <!-- Default -->
    <xf numFmtId="0" fontId="0" fillId="0" borderId="0" xfId="0"/>
    <!-- Heading -->
    <xf numFmtId="0" fontId="2" fillId="0" borderId="0" xfId="0" applyFont="true"/>
    <!-- Heading 1 -->
    <xf numFmtId="0" fontId="2" fillId="0" borderId="0" xfId="0" applyFont="true"/>
    <!-- Heading 2 -->
    <xf numFmtId="0" fontId="3" fillId="0" borderId="0" xfId="0" applyFont="true"/>
    <!-- Good status -->
    <xf numFmtId="0" fontId="4" fillId="2" borderId="0" xfId="0" applyFont="true" applyFill="true"/>
    <!-- Warning status -->
    <xf numFmtId="0" fontId="0" fillId="3" borderId="0" xfId="0" applyFill="true"/>
    <!-- Error status -->
    <xf numFmtId="0" fontId="5" fillId="4" borderId="0" xfId="0" applyFont="true" applyFill="true"/>
    <!-- Accent 1 -->
    <xf numFmtId="0" fontId="1" fillId="5" borderId="0" xfId="0" applyFont="true" applyFill="true"/>
  </cellXfs>
  
  <!-- Named cell styles -->
  <cellStyles count="7">
    <cellStyle name="Normal" xfId="0" builtinId="0"/>
    <cellStyle name="Heading" xfId="1" builtinId="16"/>
    <cellStyle name="Heading 1" xfId="2"/>
    <cellStyle name="Heading 2" xfId="3"/>
    <cellStyle name="Good" xfId="4"/>
    <cellStyle name="Warning" xfId="5"/>
    <cellStyle name="Bad" xfId="6"/>
  </cellStyles>
</styleSheet>