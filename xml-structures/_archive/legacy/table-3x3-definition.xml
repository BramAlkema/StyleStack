<?xml version="1.0" encoding="UTF-8"?>
<!-- 3x3 Table Definition with Complete OOXML Primitives -->
<!-- This provides the table structure that design tokens can patch against -->

<w:tbl xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">
  <!-- Table Properties -->
  <w:tblPr>
    <!-- Table style reference - design tokens can change this -->
    <w:tblStyle w:val="{{table.style.id}}"/>
    
    <!-- Table width - design tokens can control responsive sizing -->
    <w:tblW w:w="{{table.width.dxa}}" w:type="dxa"/>
    
    <!-- Table positioning -->
    <w:tblpPr w:leftFromText="{{table.margins.left}}" 
              w:rightFromText="{{table.margins.right}}" 
              w:vertAnchor="page" 
              w:horzAnchor="margin"
              w:tblpXSpec="{{table.align.horizontal}}"
              w:tblpY="{{table.position.y}}"/>
    
    <!-- Table layout control -->
    <w:tblLayout w:type="{{table.layout.type}}"/>  <!-- fixed, autofit -->
    
    <!-- Table indentation -->
    <w:tblInd w:w="{{table.indent.dxa}}" w:type="dxa"/>
    
    <!-- Cell spacing between cells -->
    <w:tblCellSpacing w:w="{{table.cell.spacing}}" w:type="dxa"/>
    
    <!-- Default cell margins for entire table -->
    <w:tblCellMar>
      <w:top w:w="{{table.cell.margin.top}}" w:type="dxa"/>
      <w:left w:w="{{table.cell.margin.left}}" w:type="dxa"/>
      <w:bottom w:w="{{table.cell.margin.bottom}}" w:type="dxa"/>
      <w:right w:w="{{table.cell.margin.right}}" w:type="dxa"/>
    </w:tblCellMar>
    
    <!-- Table visual properties - design tokens control banding -->
    <w:tblLook w:val="{{table.look.flags}}" 
               w:firstRow="{{table.style.firstRow}}" 
               w:lastRow="{{table.style.lastRow}}" 
               w:firstColumn="{{table.style.firstColumn}}" 
               w:lastColumn="{{table.style.lastColumn}}"
               w:noHBand="{{table.style.noHBand}}" 
               w:noVBand="{{table.style.noVBand}}"/>
    
    <!-- Table-level shading - design tokens can control background -->
    <w:shd w:val="{{table.background.pattern}}" 
           w:color="{{table.background.foreground}}" 
           w:fill="{{table.background.fill}}"/>
           
    <!-- Table borders - design tokens control all border properties -->
    <w:tblBorders>
      <w:top w:val="{{table.border.top.style}}" 
             w:sz="{{table.border.top.width}}" 
             w:space="0" 
             w:color="{{table.border.top.color}}"/>
      <w:left w:val="{{table.border.left.style}}" 
              w:sz="{{table.border.left.width}}" 
              w:space="0" 
              w:color="{{table.border.left.color}}"/>
      <w:bottom w:val="{{table.border.bottom.style}}" 
                w:sz="{{table.border.bottom.width}}" 
                w:space="0" 
                w:color="{{table.border.bottom.color}}"/>
      <w:right w:val="{{table.border.right.style}}" 
               w:sz="{{table.border.right.width}}" 
               w:space="0" 
               w:color="{{table.border.right.color}}"/>
      <w:insideH w:val="{{table.border.insideH.style}}" 
                 w:sz="{{table.border.insideH.width}}" 
                 w:space="0" 
                 w:color="{{table.border.insideH.color}}"/>
      <w:insideV w:val="{{table.border.insideV.style}}" 
                 w:sz="{{table.border.insideV.width}}" 
                 w:space="0" 
                 w:color="{{table.border.insideV.color}}"/>
    </w:tblBorders>
  </w:tblPr>
  
  <!-- Table Grid Definition - design tokens control column widths -->
  <w:tblGrid>
    <w:gridCol w:w="{{table.column1.width}}"/>
    <w:gridCol w:w="{{table.column2.width}}"/>
    <w:gridCol w:w="{{table.column3.width}}"/>
  </w:tblGrid>
  
  <!-- ROW 1 - Header Row -->
  <w:tr>
    <!-- Row properties - design tokens control row height -->
    <w:trPr>
      <w:trHeight w:val="{{table.row.header.height}}" w:hRule="{{table.row.header.heightRule}}"/>
      <w:tblHeader/>  <!-- Mark as header row -->
    </w:trPr>
    
    <!-- Cell 1,1 - Header Cell -->
    <w:tc>
      <w:tcPr>
        <!-- Cell width -->
        <w:tcW w:w="{{table.column1.width}}" w:type="dxa"/>
        <!-- Cell shading - design tokens control header background -->
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.header.background}}"/>
        <!-- Cell borders - can be different from table borders -->
        <w:tcBorders>
          <w:top w:val="{{table.header.border.top.style}}" 
                 w:sz="{{table.header.border.top.width}}" 
                 w:color="{{table.header.border.top.color}}"/>
          <w:bottom w:val="{{table.header.border.bottom.style}}" 
                    w:sz="{{table.header.border.bottom.width}}" 
                    w:color="{{table.header.border.bottom.color}}"/>
        </w:tcBorders>
        <!-- Cell margins can override table defaults -->
        <w:tcMar>
          <w:top w:w="{{table.header.cell.margin.top}}" w:type="dxa"/>
          <w:left w:w="{{table.header.cell.margin.left}}" w:type="dxa"/>
          <w:bottom w:w="{{table.header.cell.margin.bottom}}" w:type="dxa"/>
          <w:right w:w="{{table.header.cell.margin.right}}" w:type="dxa"/>
        </w:tcMar>
        <!-- Vertical alignment -->
        <w:vAlign w:val="{{table.header.cell.valign}}"/>
      </w:tcPr>
      
      <!-- Cell content - paragraph with design token controlled formatting -->
      <w:p>
        <w:pPr>
          <w:jc w:val="{{table.header.text.align}}"/>
          <w:spacing w:line="{{table.header.text.lineHeight}}" w:lineRule="exact"/>
        </w:pPr>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.header.font.family}}" 
                      w:hAnsi="{{table.header.font.family}}"/>
            <w:sz w:val="{{table.header.font.size}}"/>
            <w:szCs w:val="{{table.header.font.size}}"/>
            <w:b w:val="{{table.header.font.bold}}"/>
            <w:color w:val="{{table.header.font.color}}"/>
          </w:rPr>
          <w:t>{{content.header.cell1}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
    
    <!-- Cell 1,2 - Header Cell -->
    <w:tc>
      <w:tcPr>
        <w:tcW w:w="{{table.column2.width}}" w:type="dxa"/>
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.header.background}}"/>
        <w:vAlign w:val="{{table.header.cell.valign}}"/>
      </w:tcPr>
      <w:p>
        <w:pPr>
          <w:jc w:val="{{table.header.text.align}}"/>
        </w:pPr>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.header.font.family}}" 
                      w:hAnsi="{{table.header.font.family}}"/>
            <w:sz w:val="{{table.header.font.size}}"/>
            <w:b w:val="{{table.header.font.bold}}"/>
            <w:color w:val="{{table.header.font.color}}"/>
          </w:rPr>
          <w:t>{{content.header.cell2}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
    
    <!-- Cell 1,3 - Header Cell -->
    <w:tc>
      <w:tcPr>
        <w:tcW w:w="{{table.column3.width}}" w:type="dxa"/>
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.header.background}}"/>
        <w:vAlign w:val="{{table.header.cell.valign}}"/>
      </w:tcPr>
      <w:p>
        <w:pPr>
          <w:jc w:val="{{table.header.text.align}}"/>
        </w:pPr>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.header.font.family}}" 
                      w:hAnsi="{{table.header.font.family}}"/>
            <w:sz w:val="{{table.header.font.size}}"/>
            <w:b w:val="{{table.header.font.bold}}"/>
            <w:color w:val="{{table.header.font.color}}"/>
          </w:rPr>
          <w:t>{{content.header.cell3}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
  </w:tr>
  
  <!-- ROW 2 - Data Row (Odd) -->
  <w:tr>
    <w:trPr>
      <w:trHeight w:val="{{table.row.data.height}}" w:hRule="{{table.row.data.heightRule}}"/>
    </w:trPr>
    
    <!-- Data cells with alternating row styling -->
    <w:tc>
      <w:tcPr>
        <w:tcW w:w="{{table.column1.width}}" w:type="dxa"/>
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.row.odd.background}}"/>
        <w:vAlign w:val="{{table.data.cell.valign}}"/>
      </w:tcPr>
      <w:p>
        <w:pPr>
          <w:jc w:val="{{table.data.text.align}}"/>
        </w:pPr>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.data.font.family}}" 
                      w:hAnsi="{{table.data.font.family}}"/>
            <w:sz w:val="{{table.data.font.size}}"/>
            <w:color w:val="{{table.data.font.color}}"/>
          </w:rPr>
          <w:t>{{content.data.row2.cell1}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
    
    <w:tc>
      <w:tcPr>
        <w:tcW w:w="{{table.column2.width}}" w:type="dxa"/>
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.row.odd.background}}"/>
        <w:vAlign w:val="{{table.data.cell.valign}}"/>
      </w:tcPr>
      <w:p>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.data.font.family}}" 
                      w:hAnsi="{{table.data.font.family}}"/>
            <w:sz w:val="{{table.data.font.size}}"/>
            <w:color w:val="{{table.data.font.color}}"/>
          </w:rPr>
          <w:t>{{content.data.row2.cell2}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
    
    <w:tc>
      <w:tcPr>
        <w:tcW w:w="{{table.column3.width}}" w:type="dxa"/>
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.row.odd.background}}"/>
        <w:vAlign w:val="{{table.data.cell.valign}}"/>
      </w:tcPr>
      <w:p>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.data.font.family}}" 
                      w:hAnsi="{{table.data.font.family}}"/>
            <w:sz w:val="{{table.data.font.size}}"/>
            <w:color w:val="{{table.data.font.color}}"/>
          </w:rPr>
          <w:t>{{content.data.row2.cell3}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
  </w:tr>
  
  <!-- ROW 3 - Data Row (Even) -->
  <w:tr>
    <w:trPr>
      <w:trHeight w:val="{{table.row.data.height}}" w:hRule="{{table.row.data.heightRule}}"/>
    </w:trPr>
    
    <!-- Data cells with even row styling -->
    <w:tc>
      <w:tcPr>
        <w:tcW w:w="{{table.column1.width}}" w:type="dxa"/>
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.row.even.background}}"/>
        <w:vAlign w:val="{{table.data.cell.valign}}"/>
      </w:tcPr>
      <w:p>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.data.font.family}}" 
                      w:hAnsi="{{table.data.font.family}}"/>
            <w:sz w:val="{{table.data.font.size}}"/>
            <w:color w:val="{{table.data.font.color}}"/>
          </w:rPr>
          <w:t>{{content.data.row3.cell1}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
    
    <w:tc>
      <w:tcPr>
        <w:tcW w:w="{{table.column2.width}}" w:type="dxa"/>
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.row.even.background}}"/>
        <w:vAlign w:val="{{table.data.cell.valign}}"/>
      </w:tcPr>
      <w:p>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.data.font.family}}" 
                      w:hAnsi="{{table.data.font.family}}"/>
            <w:sz w:val="{{table.data.font.size}}"/>
            <w:color w:val="{{table.data.font.color}}"/>
          </w:rPr>
          <w:t>{{content.data.row3.cell2}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
    
    <w:tc>
      <w:tcPr>
        <w:tcW w:w="{{table.column3.width}}" w:type="dxa"/>
        <w:shd w:val="clear" w:color="auto" w:fill="{{table.row.even.background}}"/>
        <w:vAlign w:val="{{table.data.cell.valign}}"/>
      </w:tcPr>
      <w:p>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{table.data.font.family}}" 
                      w:hAnsi="{{table.data.font.family}}"/>
            <w:sz w:val="{{table.data.font.size}}"/>
            <w:color w:val="{{table.data.font.color}}"/>
          </w:rPr>
          <w:t>{{content.data.row3.cell3}}</w:t>
        </w:r>
      </w:p>
    </w:tc>
  </w:tr>
</w:tbl>