        "md": {
          "value": "{space.3}",
          "type": "dimension",
          "description": "Medium component padding",
          "$extensions": {
            "stylestack": {
              "category": "semantic"
            }
          }
        },
        "lg": {
          "value": "{space.4}",
          "type": "dimension",
          "description": "Large component padding",
          "$extensions": {
            "stylestack": {
              "category": "semantic"
