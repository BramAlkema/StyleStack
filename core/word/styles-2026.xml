<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- StyleStack 2026 Word Styles - Publication Quality Typography -->
<w:styles xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">

  <!-- Document defaults with EMU precision -->
  <w:docDefaults>
    <w:rPrDefault>
      <w:rPr>
        <w:rFonts w:ascii="Inter" w:hAnsi="Inter" w:eastAsia="Noto Sans CJK" w:cs="Noto Sans"/>
        <w:sz w:val="24"/>        <!-- 12pt base font size -->
        <w:szCs w:val="24"/>
        <w:lang w:val="en-US" w:eastAsia="ja-JP" w:bidi="ar-SA"/>
        <w:kern w:val="16"/>      <!-- Auto-kerning for 8pt+ fonts -->
        <w:spacing w:val="0"/>    <!-- Default character spacing -->
      </w:rPr>
    </w:rPrDefault>
    <w:pPrDefault>
      <w:pPr>
        <!-- 18pt baseline grid (360 twentieths) with 1.25x line height -->
        <w:spacing w:line="300" w:lineRule="auto" w:before="0" w:after="180"/>
        <w:jc w:val="left"/>
        <w:widowControl/>
      </w:pPr>
    </w:pPrDefault>
  </w:docDefaults>

  <!-- Modern paragraph styles with EMU precision -->
  
  <!-- Normal style - publication quality body text -->
  <w:style w:type="paragraph" w:default="1" w:styleId="Normal">
    <w:name w:val="Normal"/>
    <w:qFormat/>
    <w:pPr>
      <w:spacing w:line="300" w:lineRule="auto" w:before="0" w:after="180"/>
      <w:ind w:firstLine="360"/>  <!-- 18pt first line indent -->
    </w:pPr>
    <w:rPr>
      <w:rFonts w:ascii="Inter" w:hAnsi="Inter" w:eastAsia="Noto Sans CJK"/>
      <w:color w:val="1A1A1A"/>   <!-- High contrast text -->
      <w:sz w:val="24"/>          <!-- 12pt -->
      <w:kern w:val="16"/>        <!-- Auto-kerning -->
    </w:rPr>
  </w:style>

  <!-- Display Heading - 72pt with tight line spacing -->
  <w:style w:type="paragraph" w:styleId="DisplayHeading">
    <w:name w:val="Display Heading"/>
    <w:basedOn w:val="Normal"/>
    <w:qFormat/>
    <w:pPr>
      <w:spacing w:line="720" w:lineRule="exact" w:before="720" w:after="360"/>
      <w:ind w:firstLine="0"/>
      <w:jc w:val="left"/>
    </w:pPr>
    <w:rPr>
      <w:rFonts w:ascii="Inter" w:hAnsi="Inter" w:eastAsia="Noto Sans CJK"/>
      <w:color w:val="1A1A1A"/>
      <w:sz w:val="144"/>         <!-- 72pt display size -->
      <w:szCs w:val="144"/>
      <w:spacing w:val="-10"/>    <!-- Slight character compression for display -->
      <w:kern w:val="32"/>        <!-- Higher kerning threshold -->
    </w:rPr>
  </w:style>

  <!-- Heading 1 - 36pt with golden ratio spacing -->
  <w:style w:type="paragraph" w:styleId="Heading1">
    <w:name w:val="Heading 1"/>
    <w:basedOn w:val="Normal"/>
    <w:next w:val="Normal"/>
    <w:link w:val="Heading1Char"/>
    <w:qFormat/>
    <w:pPr>
      <w:keepNext/>
      <w:spacing w:line="480" w:lineRule="exact" w:before="720" w:after="240"/>
      <w:ind w:firstLine="0"/>
    </w:pPr>
    <w:rPr>
      <w:color w:val="1A1A1A"/>
      <w:sz w:val="72"/>          <!-- 36pt -->
      <w:szCs w:val="72"/>
      <w:spacing w:val="-5"/>     <!-- Optical character spacing -->
      <w:kern w:val="24"/>
    </w:rPr>
  </w:style>

  <!-- Heading 2 - 24pt with precise spacing -->
  <w:style w:type="paragraph" w:styleId="Heading2">
    <w:name w:val="Heading 2"/>
    <w:basedOn w:val="Normal"/>
    <w:next w:val="Normal"/>
    <w:link w:val="Heading2Char"/>
    <w:qFormat/>
    <w:pPr>
      <w:keepNext/>
      <w:spacing w:line="360" w:lineRule="exact" w:before="480" w:after="180"/>
      <w:ind w:firstLine="0"/>
    </w:pPr>
    <w:rPr>
      <w:color w:val="1A1A1A"/>
      <w:sz w:val="48"/>          <!-- 24pt -->
      <w:szCs w:val="48"/>
      <w:kern w:val="20"/>
    </w:rPr>
  </w:style>

  <!-- Heading 3 - 18pt baseline aligned -->
  <w:style w:type="paragraph" w:styleId="Heading3">
    <w:name w:val="Heading 3"/>
    <w:basedOn w:val="Normal"/>
    <w:next w:val="Normal"/>
    <w:link w:val="Heading3Char"/>
    <w:qFormat/>
    <w:pPr>
      <w:keepNext/>
      <w:spacing w:line="360" w:lineRule="exact" w:before="360" w:after="180"/>
      <w:ind w:firstLine="0"/>
    </w:pPr>
    <w:rPr>
      <w:color w:val="333333"/>   <!-- Slightly lighter for hierarchy -->
      <w:sz w:val="36"/>          <!-- 18pt -->
      <w:szCs w:val="36"/>
      <w:kern w:val="18"/>
    </w:rPr>
  </w:style>

  <!-- Body Text - optimized for readability -->
  <w:style w:type="paragraph" w:styleId="BodyText">
    <w:name w:val="Body Text"/>
    <w:basedOn w:val="Normal"/>
    <w:pPr>
      <w:spacing w:line="300" w:lineRule="auto" w:before="0" w:after="180"/>
      <w:ind w:firstLine="0"/>    <!-- No first line indent for modern look -->
    </w:pPr>
  </w:style>

  <!-- Caption - 9pt with tight spacing -->
  <w:style w:type="paragraph" w:styleId="Caption">
    <w:name w:val="Caption"/>
    <w:basedOn w:val="Normal"/>
    <w:pPr>
      <w:spacing w:line="240" w:lineRule="exact" w:before="120" w:after="120"/>
      <w:ind w:firstLine="0"/>
      <w:jc w:val="center"/>
    </w:pPr>
    <w:rPr>
      <w:color w:val="666666"/>   <!-- Muted color for captions -->
      <w:sz w:val="18"/>          <!-- 9pt -->
      <w:szCs w:val="18"/>
      <w:kern w:val="12"/>
    </w:rPr>
  </w:style>

  <!-- Professional Quote style -->
  <w:style w:type="paragraph" w:styleId="Quote">
    <w:name w:val="Quote"/>
    <w:basedOn w:val="Normal"/>
    <w:pPr>
      <w:spacing w:line="360" w:lineRule="exact" w:before="360" w:after="360"/>
      <w:ind w:left="720" w:right="360"/>  <!-- Indented quote -->
    </w:pPr>
    <w:rPr>
      <w:color w:val="333333"/>
      <w:i/>                      <!-- Italic for emphasis -->
      <w:sz w:val="24"/>
    </w:rPr>
  </w:style>

  <!-- Character styles for inline formatting -->
  
  <!-- Heading character styles (for inline use) -->
  <w:style w:type="character" w:styleId="Heading1Char">
    <w:name w:val="Heading 1 Char"/>
    <w:link w:val="Heading1"/>
    <w:rPr>
      <w:color w:val="1A1A1A"/>
      <w:sz w:val="72"/>
      <w:szCs w:val="72"/>
      <w:spacing w:val="-5"/>
      <w:kern w:val="24"/>
    </w:rPr>
  </w:style>

  <!-- Strong emphasis -->
  <w:style w:type="character" w:styleId="Strong">
    <w:name w:val="Strong"/>
    <w:rPr>
      <w:b/>
      <w:bCs/>
      <w:color w:val="1A1A1A"/>
    </w:rPr>
  </w:style>

  <!-- Emphasis -->
  <w:style w:type="character" w:styleId="Emphasis">
    <w:name w:val="Emphasis"/>
    <w:rPr>
      <w:i/>
      <w:iCs/>
      <w:color w:val="333333"/>
    </w:rPr>
  </w:style>

  <!-- Code/Monospace -->
  <w:style w:type="character" w:styleId="Code">
    <w:name w:val="Code"/>
    <w:rPr>
      <w:rFonts w:ascii="Fira Code" w:hAnsi="Fira Code" w:cs="Courier New"/>
      <w:color w:val="0066CC"/>   <!-- Accent color for code */
      <w:sz w:val="22"/>          <!-- Slightly smaller for monospace */
    </w:rPr>
  </w:style>

  <!-- Table styles with typography control -->
  
  <!-- Professional table style -->
  <w:style w:type="table" w:styleId="ProfessionalTable">
    <w:name w:val="Professional Table"/>
    <w:basedOn w:val="TableNormal"/>
    <w:tblPr>
      <w:tblBorders>
        <w:top w:val="single" w:sz="4" w:space="0" w:color="333333"/>
        <w:bottom w:val="single" w:sz="8" w:space="0" w:color="1A1A1A"/>
        <w:left w:val="none"/>
        <w:right w:val="none"/>
        <w:insideH w:val="single" w:sz="2" w:space="0" w:color="E0E0E0"/>
        <w:insideV w:val="none"/>
      </w:tblBorders>
      <w:tblCellMar>
        <w:top w:w="120" w:type="dxa"/>     <!-- 6pt cell padding -->
        <w:left w:w="180" w:type="dxa"/>    <!-- 9pt -->
        <w:bottom w:w="120" w:type="dxa"/>
        <w:right w:w="180" w:type="dxa"/>
      </w:tblCellMar>
    </w:tblPr>
    <w:rPr>
      <w:sz w:val="22"/>          <!-- Slightly smaller for table text -->
      <w:color w:val="1A1A1A"/>
    </w:rPr>
  </w:style>

  <!-- List styles with proper spacing -->
  
  <!-- Modern bulleted list -->
  <w:style w:type="numbering" w:styleId="ModernBullet">
    <w:name w:val="Modern Bullet"/>
    <w:basedOn w:val="Normal"/>
    <w:pPr>
      <w:spacing w:line="300" w:lineRule="auto" w:before="0" w:after="120"/>
      <w:ind w:left="360" w:hanging="180"/>  <!-- Hanging indent for bullets -->
    </w:pPr>
  </w:style>

</w:styles>