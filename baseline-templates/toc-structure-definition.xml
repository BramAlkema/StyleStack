<?xml version="1.0" encoding="UTF-8"?>
<!-- Table of Contents Structure with Complete OOXML Primitives -->
<!-- This provides the TOC structure that design tokens can patch against -->

<!-- TOC Field Definition -->
<w:fldSimple xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main" 
             w:fldLock="0" 
             w:dirty="0" 
             w:instr="TOC \o &quot;{{toc.outline.levels}}&quot; \h \z \u">
  
  <!-- TOC Hyperlink Field -->
  <w:hyperlink w:anchor="{{toc.bookmark.prefix}}_Toc{{toc.entry.id}}" 
               w:history="1">
    <w:r>
      <w:rPr>
        <!-- TOC entry formatting controlled by design tokens -->
        <w:rFonts w:ascii="{{toc.font.family}}" 
                  w:hAnsi="{{toc.font.family}}" 
                  w:cs="{{toc.font.family}}"/>
        <w:sz w:val="{{toc.font.size}}"/>
        <w:szCs w:val="{{toc.font.size}}"/>
        <w:color w:val="{{toc.font.color}}"/>
        <!-- Hyperlink underlining can be controlled -->
        <w:u w:val="{{toc.hyperlink.underline}}"/>
      </w:rPr>
      <w:t>{{toc.entry.text}}</w:t>
    </w:r>
    
    <!-- Tab character to page number -->
    <w:r>
      <w:rPr>
        <w:webHidden/>
      </w:rPr>
      <w:tab/>
    </w:r>
    
    <!-- Page number -->
    <w:r>
      <w:rPr>
        <w:rFonts w:ascii="{{toc.font.family}}" 
                  w:hAnsi="{{toc.font.family}}"/>
        <w:sz w:val="{{toc.font.size}}"/>
        <w:color w:val="{{toc.font.color}}"/>
        <w:u w:val="{{toc.hyperlink.underline}}"/>
      </w:rPr>
      <w:t>{{toc.entry.pageNumber}}</w:t>
    </w:r>
  </w:hyperlink>
  
  <!-- Alternative non-hyperlink version -->
  <w:r>
    <w:rPr>
      <w:rFonts w:ascii="{{toc.font.family}}" 
                w:hAnsi="{{toc.font.family}}"/>
      <w:sz w:val="{{toc.font.size}}"/>
      <w:color w:val="{{toc.font.color}}"/>
    </w:rPr>
    <w:t>{{toc.entry.text}}</w:t>
  </w:r>
  
  <w:r>
    <w:rPr>
      <w:webHidden/>
    </w:rPr>
    <w:tab/>
  </w:r>
  
  <w:r>
    <w:rPr>
      <w:rFonts w:ascii="{{toc.font.family}}" 
                w:hAnsi="{{toc.font.family}}"/>
      <w:sz w:val="{{toc.font.size}}"/>
      <w:color w:val="{{toc.font.color}}"/>
    </w:rPr>
    <w:t>{{toc.entry.pageNumber}}</w:t>
  </w:r>
</w:fldSimple>

<!-- Complete TOC Document Structure -->
<w:document xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">
  <w:body>
    
    <!-- TOC Heading -->
    <w:p>
      <w:pPr>
        <!-- Use TOCHeading style defined in complete-word-styles.xml -->
        <w:pStyle w:val="TOCHeading"/>
        <w:spacing w:before="{{toc.heading.spacing.before}}" 
                   w:after="{{toc.heading.spacing.after}}"/>
        <w:jc w:val="{{toc.heading.alignment}}"/>
        <w:keepNext/>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="{{toc.heading.font.family}}" 
                    w:hAnsi="{{toc.heading.font.family}}"/>
          <w:sz w:val="{{toc.heading.font.size}}"/>
          <w:b w:val="{{toc.heading.font.bold}}"/>
          <w:color w:val="{{toc.heading.font.color}}"/>
        </w:rPr>
        <w:t>{{toc.heading.text}}</w:t>
      </w:r>
    </w:p>
    
    <!-- TOC Level 1 Entry -->
    <w:p>
      <w:pPr>
        <w:pStyle w:val="TOC1"/>
        <w:spacing w:after="{{toc.level1.spacing.after}}"/>
        <!-- Tab stops for leader dots -->
        <w:tabs>
          <w:tab w:val="right" 
                 w:leader="{{toc.level1.tab.leader}}" 
                 w:pos="{{toc.level1.tab.position}}"/>
        </w:tabs>
      </w:pPr>
      
      <!-- TOC Entry Content -->
      <w:hyperlink w:anchor="{{toc.bookmark.prefix}}_Toc1" w:history="1">
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{toc.level1.font.family}}" 
                      w:hAnsi="{{toc.level1.font.family}}"/>
            <w:sz w:val="{{toc.level1.font.size}}"/>
            <w:b w:val="{{toc.level1.font.bold}}"/>
            <w:color w:val="{{toc.level1.font.color}}"/>
            <w:u w:val="{{toc.level1.hyperlink.underline}}"/>
          </w:rPr>
          <w:t>{{toc.level1.entry1.text}}</w:t>
        </w:r>
        <w:r>
          <w:rPr><w:webHidden/></w:rPr>
          <w:tab/>
        </w:r>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{toc.level1.font.family}}" 
                      w:hAnsi="{{toc.level1.font.family}}"/>
            <w:sz w:val="{{toc.level1.font.size}}"/>
            <w:b w:val="{{toc.level1.font.bold}}"/>
            <w:color w:val="{{toc.level1.font.color}}"/>
            <w:u w:val="{{toc.level1.hyperlink.underline}}"/>
          </w:rPr>
          <w:t>{{toc.level1.entry1.pageNumber}}</w:t>
        </w:r>
      </w:hyperlink>
    </w:p>
    
    <!-- TOC Level 2 Entry -->
    <w:p>
      <w:pPr>
        <w:pStyle w:val="TOC2"/>
        <w:spacing w:after="{{toc.level2.spacing.after}}"/>
        <w:ind w:left="{{toc.level2.indent}}"/>
        <w:tabs>
          <w:tab w:val="right" 
                 w:leader="{{toc.level2.tab.leader}}" 
                 w:pos="{{toc.level2.tab.position}}"/>
        </w:tabs>
      </w:pPr>
      
      <w:hyperlink w:anchor="{{toc.bookmark.prefix}}_Toc2" w:history="1">
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{toc.level2.font.family}}" 
                      w:hAnsi="{{toc.level2.font.family}}"/>
            <w:sz w:val="{{toc.level2.font.size}}"/>
            <w:b w:val="{{toc.level2.font.bold}}"/>
            <w:color w:val="{{toc.level2.font.color}}"/>
            <w:u w:val="{{toc.level2.hyperlink.underline}}"/>
          </w:rPr>
          <w:t>{{toc.level2.entry1.text}}</w:t>
        </w:r>
        <w:r>
          <w:rPr><w:webHidden/></w:rPr>
          <w:tab/>
        </w:r>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{toc.level2.font.family}}" 
                      w:hAnsi="{{toc.level2.font.family}}"/>
            <w:sz w:val="{{toc.level2.font.size}}"/>
            <w:color w:val="{{toc.level2.font.color}}"/>
            <w:u w:val="{{toc.level2.hyperlink.underline}}"/>
          </w:rPr>
          <w:t>{{toc.level2.entry1.pageNumber}}</w:t>
        </w:r>
      </w:hyperlink>
    </w:p>
    
    <!-- TOC Level 3 Entry -->
    <w:p>
      <w:pPr>
        <w:pStyle w:val="TOC3"/>
        <w:spacing w:after="{{toc.level3.spacing.after}}"/>
        <w:ind w:left="{{toc.level3.indent}}"/>
        <w:tabs>
          <w:tab w:val="right" 
                 w:leader="{{toc.level3.tab.leader}}" 
                 w:pos="{{toc.level3.tab.position}}"/>
        </w:tabs>
      </w:pPr>
      
      <w:hyperlink w:anchor="{{toc.bookmark.prefix}}_Toc3" w:history="1">
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{toc.level3.font.family}}" 
                      w:hAnsi="{{toc.level3.font.family}}"/>
            <w:sz w:val="{{toc.level3.font.size}}"/>
            <w:b w:val="{{toc.level3.font.bold}}"/>
            <w:color w:val="{{toc.level3.font.color}}"/>
            <w:u w:val="{{toc.level3.hyperlink.underline}}"/>
          </w:rPr>
          <w:t>{{toc.level3.entry1.text}}</w:t>
        </w:r>
        <w:r>
          <w:rPr><w:webHidden/></w:rPr>
          <w:tab/>
        </w:r>
        <w:r>
          <w:rPr>
            <w:rFonts w:ascii="{{toc.level3.font.family}}" 
                      w:hAnsi="{{toc.level3.font.family}}"/>
            <w:sz w:val="{{toc.level3.font.size}}"/>
            <w:color w:val="{{toc.level3.font.color}}"/>
            <w:u w:val="{{toc.level3.hyperlink.underline}}"/>
          </w:rPr>
          <w:t>{{toc.level3.entry1.pageNumber}}</w:t>
        </w:r>
      </w:hyperlink>
    </w:p>
    
    <!-- Heading Bookmarks that TOC links to -->
    <w:p>
      <w:pPr>
        <w:pStyle w:val="Heading1"/>
      </w:pPr>
      <w:bookmarkStart w:id="{{toc.bookmark.id1}}" w:name="{{toc.bookmark.prefix}}_Toc1"/>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="{{typography.font.heading}}" 
                    w:hAnsi="{{typography.font.heading}}"/>
          <w:sz w:val="{{typography.size.h1}}"/>
          <w:b/>
          <w:color w:val="{{color.heading.primary}}"/>
        </w:rPr>
        <w:t>{{content.heading1.text}}</w:t>
      </w:r>
      <w:bookmarkEnd w:id="{{toc.bookmark.id1}}"/>
    </w:p>
    
    <!-- Additional content paragraphs -->
    <w:p>
      <w:pPr>
        <w:pStyle w:val="Normal"/>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="{{typography.font.body}}" 
                    w:hAnsi="{{typography.font.body}}"/>
          <w:sz w:val="{{typography.size.body}}"/>
          <w:color w:val="{{color.text.primary}}"/>
        </w:rPr>
        <w:t>{{content.body.paragraph1}}</w:t>
      </w:r>
    </w:p>
    
    <!-- Heading 2 with bookmark -->
    <w:p>
      <w:pPr>
        <w:pStyle w:val="Heading2"/>
      </w:pPr>
      <w:bookmarkStart w:id="{{toc.bookmark.id2}}" w:name="{{toc.bookmark.prefix}}_Toc2"/>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="{{typography.font.heading}}" 
                    w:hAnsi="{{typography.font.heading}}"/>
          <w:sz w:val="{{typography.size.h2}}"/>
          <w:b/>
          <w:color w:val="{{color.heading.primary}}"/>
        </w:rPr>
        <w:t>{{content.heading2.text}}</w:t>
      </w:r>
      <w:bookmarkEnd w:id="{{toc.bookmark.id2}}"/>
    </w:p>
    
    <!-- Heading 3 with bookmark -->
    <w:p>
      <w:pPr>
        <w:pStyle w:val="Heading3"/>
      </w:pPr>
      <w:bookmarkStart w:id="{{toc.bookmark.id3}}" w:name="{{toc.bookmark.prefix}}_Toc3"/>
      <w:r>
        <w:rPr>
          <w:rFonts w:ascii="{{typography.font.heading}}" 
                    w:hAnsi="{{typography.font.heading}}"/>
          <w:sz w:val="{{typography.size.h3}}"/>
          <w:b/>
          <w:color w:val="{{color.heading.primary}}"/>
        </w:rPr>
        <w:t>{{content.heading3.text}}</w:t>
      </w:r>
      <w:bookmarkEnd w:id="{{toc.bookmark.id3}}"/>
    </w:p>
    
  </w:body>
</w:document>

<!-- TOC Field Code for Automatic Generation -->
<!--
  Alternative TOC field codes that design tokens can control:
  
  TOC \o "1-3" \h \z \u
  - \o "1-3": Include outline levels 1-3 
  - \h: Create hyperlinks
  - \z: Hide tab leader and page numbers in Web Layout view
  - \u: Use outline levels instead of TC fields
  
  TOC \h \z \t "Heading 1;1;Heading 2;2;Heading 3;3"
  - Use specific styles with levels
  
  TOC \o "1-9" \h \z \u \f
  - \f: Include only in printed document
  
  Design tokens can control:
  - Which outline levels to include: {{toc.outline.levels}}
  - Whether to include hyperlinks: {{toc.include.hyperlinks}}
  - Tab leader style: {{toc.tab.leader}}
  - Which styles to include: {{toc.styles.list}}
-->